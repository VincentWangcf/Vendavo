<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
  	<groupId>com.avnet.emasia</groupId>
  	<artifactId>webquote</artifactId>
  	<version>2.6</version>
  	<relativePath>../webquote/pom.xml</relativePath>
  </parent>

  <artifactId>WQ_RPT_EJB_Reports</artifactId>
  <packaging>ejb</packaging>

  <dependencies>


	<dependency>
		<groupId>com.avnet.emasia</groupId>
		<artifactId>WQ_EJB_Quote</artifactId>
		<type>ejb</type>
	</dependency>

	<dependency>
		<groupId>com.avnet.emasia</groupId>
		<artifactId>WQ_JPA_Entity</artifactId>
		<type>ejb</type>
	</dependency>

	<dependency>
		<groupId>com.avnet.emasia</groupId>
		<artifactId>WQ_EJB_Utility</artifactId>
		<type>ejb</type>			
	</dependency>

	<dependency>
		<groupId>com.avnet.emasia</groupId>
		<artifactId>WQ_EJB_UserAndAuthor</artifactId>
		<type>ejb</type>			
	</dependency>

  
    <dependency>
      <groupId>org.apache.poi</groupId>
      <artifactId>poi</artifactId>
    </dependency>
  
    <dependency>
      <groupId>org.apache.poi</groupId>
      <artifactId>poi-ooxml</artifactId>
    </dependency>
  
    <dependency>
      <groupId>org.apache.poi</groupId>
      <artifactId>poi-ooxml-schemas</artifactId>
    </dependency>
  
    <dependency>
      <groupId>org.apache.poi</groupId>
      <artifactId>poi-scratchpad</artifactId>
    </dependency>
    
	<dependency>
		<groupId>commons-beanutils</groupId>
		<artifactId>commons-beanutils</artifactId>
	</dependency>


	<dependency>
		<groupId>org.eclipse.persistence</groupId>
		<artifactId>eclipselink</artifactId>
	</dependency>

	<dependency>
		<groupId>org.jboss.spec.javax.ejb</groupId>
		<artifactId>jboss-ejb-api_3.2_spec</artifactId>
	</dependency>

	<dependency>
		<groupId>org.jboss.ejb3</groupId>
		<artifactId>jboss-ejb3-ext-api</artifactId>
	</dependency>


	<dependency>
		<groupId>com.sun.mail</groupId>
		<artifactId>javax.mail</artifactId>
	</dependency>

	<dependency>
		<groupId>jboss</groupId>
		<artifactId>jboss-annotations-ejb3</artifactId>
	</dependency>


	<dependency>
		<groupId>org.jboss.spec.javax.transaction</groupId>
		<artifactId>jboss-transaction-api_1.2_spec</artifactId>
	</dependency>

    
  </dependencies>
  
  
  
  <build>

    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.5.1</version>
        <configuration>
		  <fileNameMapping>no-version</fileNameMapping>
		  <finalName>WebQuoteRptEar</finalName>
          
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-ejb-plugin</artifactId>
        <version>2.5</version>
        <configuration>
          <ejbVersion>3.2</ejbVersion>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>