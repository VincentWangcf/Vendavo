package com.avnet.emasia.webquote.exception;

/**
 * The Class WebQuoteException.
 */
public class WebQuoteException extends AbstractBaseException {

	/** The Constant serialVersionUID. */
	private static final long serialVersionUID = -6387620855096632064L;

	/**
	 * Instantiates a new web quote exception.
	 *
	 * @param errorCode the error
	 * @param causedBy the caused by
	 */
	public WebQuoteException(final String errorCode, final Exception causedBy) {
		super(errorCode, causedBy);
	}

	public WebQuoteException() {
		super();
	}

	public WebQuoteException(String errorCode, Object[] parameters) {
		super(errorCode, parameters);
	}

	public WebQuoteException(String errorCode, Throwable causedBy, Object[] parameters) {
		super(errorCode, causedBy, parameters);
	}

	public WebQuoteException(String errorCode, Throwable causedBy) {
		super(errorCode, causedBy);
	}

	public WebQuoteException(String errorCode) {
		super(errorCode);
	}

	public WebQuoteException(Throwable causedBy) {
		super(causedBy);
	}

	public WebQuoteException(String errorCode,String mainMessage,String message) {
		super(errorCode,mainMessage,message);
	}
	

}
