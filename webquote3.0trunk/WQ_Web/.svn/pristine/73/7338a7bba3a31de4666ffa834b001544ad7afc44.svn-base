							value="#{dbResource['wq.button.search']}"
							update="alternatePartSearch_panel"
							actionListener="#{rfqSubmissionMB.searchInMaterialPopup()}"
							style="margin:0px 0px 0px 0px" global="false" onstart="start();"
							oncomplete="stop();" onerror="stopWithError();" />
						<p:defaultCommand target="validation_expansion_SearchButton" />
					</h:panelGrid>

					<p:dataTable var="search_alternate_rfq"
						value="#{rfqSubmissionMB.partModel}"
						selection="#{rfqSubmissionMB.selectedSearchParts}"
						rowIndexVar="rowIndex" id="datatable_search_alternate"
						paginator="true" rows="10" rowsPerPageTemplate="5,10,20"
						paginatorPosition="top" style="margin:5px 0px 0px 0px;"
						emptyMessage="#{dbResource['wq.message.noRecordFound']}.">

						<p:column headerText="#{dbResource['wq.label.no']}"
							id="search_alternate_rowNumber">
					        #{search_alternate_rfq.itemNumber}
				        </p:column>

						<p:column selectionMode="multiple" />

						<p:column headerText="#{dbResource['wq.label.mfr']}"
							id="search_alternate_mfr">
					        #{search_alternate_rfq.mfr}
				        </p:column>

						<p:column headerText="#{dbResource['wq.label.mfrP']}"
							id="search_alternate_fullMfrPart">
					        #{search_alternate_rfq.fullMfrPartNumber}
				        </p:column>

						<p:column headerText="#{dbResource['wq.label.MPQ']}"
							id="search_alternate_mpq">
							#{rfqSubmissionMB.convertNumber(search_alternate_rfq.mpq)}
				        </p:column>

						<p:column headerText="#{dbResource['wq.label.moq']}"
							id="search_alternate_moq">
							#{rfqSubmissionMB.convertNumber(search_alternate_rfq.moq)}
				        </p:column>

						<p:column headerText="#{dbResource['wq.label.leadTime']}"
							id="search_alternate_leadTime">
							#{search_alternate_rfq.leadTime}
				        </p:column>

						<p:column headerText="#{dbResource['wq.label.description']}"
							id="search_alternate_description">
							#{search_alternate_rfq.description}
				        </p:column>
					</p:dataTable>

					<p:commandButton id="validation_expansion_SelectButton"
						value="#{dbResource['wq.button.select']}"
						actionListener="#{rfqSubmissionMB.addParts}"
						update=":form_rfqSubmission:datatable_basicDetails_rfqSubmission"
						style="margin:0px 0px 10px 0px"
						oncomplete="PF('validation_dialog').hide();stop();" global="false"
						onstart="start();" onerror="stopWithError();" />

				</p:panel>

			</p:dialog>
			<p:remoteCommand name="callToBean"
				actionListener="#{rfqSubmissionMB.checkVerifiedPopupWindow}" />

		</h:form>
		<script type="text/javascript"> 	
function nextButtonChange(tabId)
{
	
	var nextBut = document.getElementById("form_rfqSubmission:nextButton");
	var backBut = document.getElementById("form_rfqSubmission:backButton");
	if(tabId==0)
	{
	   backBut.style.display='';	
	   nextBut.style.display='';	
	}
	
	if(tabId==1)
	{
		backBut.style.display='';	
		nextBut.style.display='none';	
	}

}

function backButtonChange(tabId)
{

	var nextBut = document.getElementById("form_rfqSubmission:nextButton");
	var backBut = document.getElementById("form_rfqSubmission:backButton");
	
	if(tabId==1)
	{
		backBut.style.display='none';	
		nextBut.style.display='';	
	}
    if(tabId==2)
	{
		backBut.style.display='';	
		nextBut.style.display='';	
	}
}

function checkConfirmation(xhr, status, args){
	if(args.comfirmFlag == '1'){
		PF('confirm_select_bmt_dialog').show();
		stop();
		}else if(args.comfirmFlag == '0'){
			PF('confirm_submit_dialog').show();
			stop();
			}
}


 	
window.onload =function dosomeThing()
{
	var nextBut = document.getElementById("form_rfqSubmission:nextButton");
	var backBut = document.getElementById("form_rfqSubmission:backButton");
	var tabId = PF('wiz').getStepIndex(PF('wiz').currentStep);
	if(tabId==2)
	{
		backBut.style.display='';	
		nextBut.style.display='none';	
	}
	callToBean();
	
}
</script>

	</h:body>
</f:view>

</html>
