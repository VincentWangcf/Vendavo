<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3c.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui"
	xmlns:c="http://java.sun.com/jsp/jstl/core">

<f:view contentType="text/html">
	<h:head>
		<f:facet name="first">
			<meta http-equiv="X-UA-Compatible" content="IE=edge" />
			<meta content='text/html; charset=UTF-8' http-equiv="Content-Type" />
			<meta http-equiv="Cache-Control" Content="no-cache" />
			<meta http-equiv="Pragma" Content="no-cache" />
			<meta http-equiv="Expires" Content="0" />
			<title><h:outputText
					value="#{dbResource['wq.title.webQuoteVersion']}" /></title>
		</f:facet>
		<f:view locale="#{userMB.user.userLocale}"></f:view>
	</h:head>
	<h:body>

		<h:form id="form_rfqSubmission" enctype="multipart/form-data"
			acceptcharset="UTF-8"
			onkeypress="if (event.keyCode == 13) {document.getElementById('form_rfqSubmission:basicDetails_soldToCustomerNumber').blur(); return false; }">
			<p:wizard widgetVar="wiz" id ="widgetPanelGridID"
				flowListener="#{rfqSubmissionMB.onFlowProcess}" showNavBar='false'
				onback="backButtonChange(PF('wiz').getStepIndex(PF('wiz').currentStep));"
				onnext="nextButtonChange(PF('wiz').getStepIndex(PF('wiz').currentStep));">
				<p:tab id="basicDetails"
					title="1. #{dbResource['wq.header.rfqHeader']}">
					<p:growl id="RfqSubmissionGrowl" for="RfqSubmissionGrowl_1"
						sticky="{growlPropertyMB.getSticky()}"
						showSummary="#{growlPropertyMB.getShowSummary()}"
						showDetail="true" escape="#{growlPropertyMB.getEscape()}" />
					<p:panel header="#{dbResource['wq.header.rfqHeader']}"
						id="basicDetails_rfqDetails" style="font-size:12pt">
						<h:panelGrid columns="22" cellpadding="0"
							style="padding:4px 3px 4px 3px;">

							<h:outputText value="#{dbResource['wq.label.region']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value="&#042;"
								style="color:#ff0000; padding:0px 0px 0px 0px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:spacer width="30" height="10" />
							<h:selectOneMenu id="basicDetails_bizUnit"
								value="#{rfqSubmissionMB.rfqHeader.bizUnit}"
								disabled="#{rfqSubmissionMB.rfqHeader.disableRegion()}">
								<f:selectItems value="#{rfqSubmissionMB.bizUnitSelectList}" />
								<p:ajax event="change" update="@form"
									listener="#{rfqSubmissionMB.updateBizUnit}"
									oncomplete="stop();" global="false" onstart="start();"
									onerror="stopWithError();" />
							</h:selectOneMenu>
							<p:spacer width="3" height="10" />
							<h:outputText value="#{dbResource['wq.label.salesmanEmpCode']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value="&#042;"
								style="color:#ff0000; padding:0px 0px 0px 0px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:autoComplete id="basicDetails_salesEmployeeNumber"
								queryDelay="300" cache="false" cacheTimeout="30000"
								minQueryLength="3" maxResults="10"
								value="#{rfqSubmissionMB.rfqHeader.salesEmployeeNumber}"
								completeMethod="#{rfqSubmissionMB.autoCompleteSalesNumber}"
								rendered="#{!rfqSubmissionMB.sales}">
								<p:ajax event="itemSelect"
									update=":form_rfqSubmission:cs_panel,:form_rfqSubmission:basicDetails_salesEmployeeName,:form_rfqSubmission:basicDetails_salesEmployeeNumber,:form_rfqSubmission:basicDetails_salesTeam,:form_rfqSubmission:basicDetails_copyToCs,:form_rfqSubmission:basicDetails_bizUnit,@form,uploadPanel,datatable_basicDetails_rfqSubmission"
									listener="#{rfqSubmissionMB.updateSalesName}" />
							</p:autoComplete>
							<p:inputText
								value="#{rfqSubmissionMB.rfqHeader.salesEmployeeNumber}"
								readonly="true" rendered="#{rfqSubmissionMB.sales}"
								style="color: black;background-color: #dddddd;" />

							<h:outputText value="#{dbResource['wq.label.salesman']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value="&#042;"
								style="color:#ff0000; padding:0px 0px 0px 0px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:autoComplete id="basicDetails_salesEmployeeName"
								queryDelay="300" cache="true" cacheTimeout="30000"
								minQueryLength="5" maxResults="10"
								value="#{rfqSubmissionMB.rfqHeader.salesEmployeeName}"
								completeMethod="#{rfqSubmissionMB.autoCompleteSalesName}"
								rendered="#{!rfqSubmissionMB.sales}">
								<p:ajax event="itemSelect"
									update=":form_rfqSubmission:cs_panel,:form_rfqSubmission:basicDetails_salesEmployeeNumber,:form_rfqSubmission:basicDetails_salesEmployeeName,:form_rfqSubmission:basicDetails_salesTeam,:form_rfqSubmission:basicDetails_copyToCs,:form_rfqSubmission:basicDetails_bizUnit,@form"
									listener="#{rfqSubmissionMB.updateSalesNumber}" />
							</p:autoComplete>
							<p:inputText
								value="#{rfqSubmissionMB.rfqHeader.salesEmployeeName}"
								rendered="#{rfqSubmissionMB.sales}" readonly="true"
								style="color: black;background-color: #dddddd;" />

							<h:outputText value="#{dbResource['wq.label.team']}: "
								style="padding:0px 3px 0px 3px" />
							<p:inputText id="basicDetails_salesTeam"
								value="#{rfqSubmissionMB.rfqHeader.team}"
								styleClass="input_text_half" readonly="true"
								rendered="#{!rfqSubmissionMB.sales}"
								style="width:250px !important;color: black;background-color: #dddddd;" />
							<p:inputText value="#{rfqSubmissionMB.rfqHeader.team}"
								readonly="true" rendered="#{rfqSubmissionMB.sales}"
								style="width:250px !important;color: black;background-color: #dddddd;" />





						</h:panelGrid>
						<h:panelGrid columns="22" cellpadding="0"
							style="padding:4px 3px 4px 3px;">

							<h:outputText value="#{dbResource['wq.label.SoldToCode']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value="&#042;"
								style="color:#ff0000; padding:0px 0px 0px 0px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:inputText id="basicDetails_soldToCustomerNumber"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								value="#{rfqSubmissionMB.rfqHeader.soldToCustomerNumber}">
								<p:ajax event="blur"
									update=":form_rfqSubmission:basicDetails_customerType,:form_rfqSubmission:basicDetails_soldToCustomerNumber,:form_rfqSubmission:basicDetails_soldToCustomerName,:form_rfqSubmission:basicDetails_chinese_soldToCustomerName,:form_rfqSubmission"
									listener="#{rfqSubmissionMB.updateSoldToCustomerName}" />
							</p:inputText>

							<h:outputText value="#{dbResource['wq.label.SoldToParty']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:inputText id="basicDetails_soldToCustomerName"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								value="#{rfqSubmissionMB.rfqHeader.soldToCustomerName}"
								readonly="true" style="color: black;background-color: #dddddd;">
								<p:tooltip id="toolForBasicDetails_soldToCustomerName"
									for="basicDetails_soldToCustomerName"
									value="#{rfqSubmissionMB.rfqHeader.soldToCustomerName}" />
							</p:inputText>
							<p:commandLink id="soldToCodeSearchIcon"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								action="#{rfqSubmissionMB.changeToSoldToCodeSearch}"
								update=":form_rfqSubmission_customer:customerSearch_panel"
								oncomplete="PF('customer_dialog').show(); handleResizeDialog(PF('customer_dialog'));">
								<h:graphicImage url="./../resources/images/icon_magnifier.png"
									styleClass="iconLink"
									title="#{rfqSubmissionMB.emptySalesManMessage()}" />
							</p:commandLink>

							<h:outputText value="" style="padding:0px 3px 0px 3px" />
							<p:commandButton value="#{dbResource['wq.label.crtNewCustmr']}"
								disabled="#{!rfqSubmissionMB.createProspectiveCustomerButtonDisplay or rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								actionListener="#{rfqSubmissionMB.updateNewProspectiveCustomer}"
								oncomplete="PF('newProspectiveCustomer_dialog').show()"
								update=":form_rfqSubmission:confirmation_dialog,:form_rfqSubmission:newProspectiveCustomer_dialog,:form_rfqSubmission:newProspectiveCustomer_customerName,:form_rfqSubmission:newProspectiveCustomer_country,:form_rfqSubmission:newProspectiveCustomer_city,:form_rfqSubmission:newProspectiveCustomer_salesOrg,:form_rfqSubmission:newProspectiveCustomer_address"
								rendered="#{!rfqSubmissionMB.qcp}" />
							<p:spacer width="20" height="10" />
							<h:outputText value="#{dbResource['wq.label.chineseCustName']}:"
								style="padding:0px 3px 0px 3px" />
							<p:inputText id="basicDetails_chinese_soldToCustomerName"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								value="#{rfqSubmissionMB.rfqHeader.chineseSoldToCustomerName}" />

							<h:outputText value="#{dbResource['wq.label.custmrType']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value="&#042;"
								style="color:#ff0000; padding:0px 0px 0px 0px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<h:selectOneMenu id="basicDetails_customerType"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								value="#{rfqSubmissionMB.rfqHeader.customerType}">
								<f:selectItems value="#{rfqSubmissionMB.customerTypeSelectList}" />
							</h:selectOneMenu>

						</h:panelGrid>
						<h:panelGrid id="customerPanelGrid" columns="18" cellpadding="0"
							style="padding:4px 3px 4px 3px;">

							<h:outputText value="#{dbResource['wq.label.ShipToCode']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:spacer width="7" height="10" />
							<p:autoComplete id="basicDetails_shipToCustomerNumber"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								queryDelay="300" cache="true" cacheTimeout="30000"
								minQueryLength="3" maxResults="10"
								value="#{rfqSubmissionMB.rfqHeader.shipToCustomerNumber}"
								completeMethod="#{rfqSubmissionMB.autoCompleteShipToCustomerNumberLabelList}">
								<p:ajax event="itemSelect"
									update=":form_rfqSubmission:basicDetails_shipToCustomerName,:form_rfqSubmission:basicDetails_shipToCustomerNumber,:form_rfqSubmission:customerPanelGrid"
									listener="#{rfqSubmissionMB.updateShipToCustomerName}" />
							</p:autoComplete>


							<h:outputText value="#{dbResource['wq.label.ShipToParty']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:autoComplete id="basicDetails_shipToCustomerName"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								queryDelay="300" cache="true" cacheTimeout="30000"
								minQueryLength="5" maxResults="10"
								value="#{rfqSubmissionMB.rfqHeader.shipToCustomerName}"
								completeMethod="#{rfqSubmissionMB.autoCompleteShipToCustomerNameLabelList}"
								focus="formAttachmentName">
								<p:ajax event="itemSelect"
									update=":form_rfqSubmission:basicDetails_shipToCustomerName,:form_rfqSubmission:basicDetails_shipToCustomerNumber,:form_rfqSubmission:customerPanelGrid"
									listener="#{rfqSubmissionMB.updateShipToCustomerNumber}" />
							</p:autoComplete>
							<p:tooltip id="toolForBasicDetails_shipToCustomerName"
								for="basicDetails_shipToCustomerName"
								value="#{rfqSubmissionMB.rfqHeader.shipToCustomerName}" />
							<p:commandLink
								action="#{rfqSubmissionMB.changeToShipToCodeSearch}"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								update=":form_rfqSubmission_customer:customerSearch_panel"
								oncomplete="PF('customer_dialog').show()">
								<h:graphicImage url="./../resources/images/icon_magnifier.png"
									styleClass="iconLink"
									title="#{rfqSubmissionMB.emptySalesManMessage()}" />
							</p:commandLink>

							<h:outputText value="#{dbResource['wq.label.EndCustomerCode']}:"
								style="padding:0px 3px 0px 3px" />
							<p:autoComplete id="basicDetails_endCustomerNumber"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								queryDelay="300" cache="true" cacheTimeout="30000"
								minQueryLength="3" maxResults="10"
								value="#{rfqSubmissionMB.rfqHeader.endCustomerNumber}" size="10"
								completeMethod="#{rfqSubmissionMB.autoCompleteEndCustomerNumberLabelList}">
								<p:ajax event="itemSelect"
									update=":form_rfqSubmission:basicDetails_endCustomerName,:form_rfqSubmission:basicDetails_endCustomerNumber,:form_rfqSubmission:customerPanelGrid"
									listener="#{rfqSubmissionMB.updateEndCustomerName}" />
							</p:autoComplete>

							<h:outputText value="#{dbResource['wq.label.endCust']}:"
								style="padding:0px 3px 0px 3px" />
							<p:autoComplete id="basicDetails_endCustomerName"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								queryDelay="300" cache="true" cacheTimeout="30000"
								minQueryLength="5" maxResults="10"
								value="#{rfqSubmissionMB.rfqHeader.endCustomerName}"
								completeMethod="#{rfqSubmissionMB.autoCompleteEndCustomerNameLabelList}">
								<p:ajax event="itemSelect"
									update=":form_rfqSubmission:basicDetails_endCustomerName,:form_rfqSubmission:toolForBasicDetails_endCustomerName,:form_rfqSubmission:customerPanelGrid"
									listener="#{rfqSubmissionMB.updateEndCustomerNumber}" />
								<p:ajax event="change"
									update=":form_rfqSubmission:basicDetails_endCustomerName,:form_rfqSubmission:basicDetails_endCustomerNumber,:form_rfqSubmission:customerPanelGrid"
									listener="#{rfqSubmissionMB.updateEndCustomerNumber}" />
							</p:autoComplete>
							<p:tooltip id="toolForBasicDetails_endCustomerName"
								for="basicDetails_endCustomerName"
								value="#{rfqSubmissionMB.rfqHeader.endCustomerName}" />
							<p:commandLink
								action="#{rfqSubmissionMB.changeToEndCustomerCodeSearch}"
								disabled="#{rfqSubmissionMB.emptySalesMan()}"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								update=":form_rfqSubmission_customer:customerSearch_panel"
								oncomplete="PF('customer_dialog').show();stop();" global="false"
								onstart="start();" onerror="stopWithError();">
								<h:graphicImage url="./../resources/images/icon_magnifier.png"
									styleClass="iconLink" />
							</p:commandLink>

							<h:outputText value="" style="padding:0px 3px 0px 3px" />
							<p:commandButton value="#{dbResource['wq.label.crtEndCust']}"
								disabled="#{!rfqSubmissionMB.createProspectiveCustomerButtonDisplay or rfqSubmissionMB.emptySalesMan() }"
								title="#{rfqSubmissionMB.emptySalesManMessage()}"
								actionListener="#{rfqSubmissionMB.updateNewProspectiveEndCustomer}"
								oncomplete="PF('newProspectiveCustomer_dialog').show()"
								update=":form_rfqSubmission:confirmation_dialog,:form_rfqSubmission:newProspectiveCustomer_dialog,:form_rfqSubmission:newProspectiveCustomer_customerName,:form_rfqSubmission:newProspectiveCustomer_country,:form_rfqSubmission:newProspectiveCustomer_city,:form_rfqSubmission:newProspectiveCustomer_salesOrg,:form_rfqSubmission:newProspectiveCustomer_address"
								rendered="#{!rfqSubmissionMB.qcp}" />
							<p:spacer width="20" height="10" />



						</h:panelGrid>
						<h:panelGrid columns="18" cellpadding="0"
							style="padding:4px 3px 4px 3px;">

							<h:outputText value="#{dbResource['wq.label.segment']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value="&#042;"
								style="color:#ff0000; padding:0px 0px 0px 0px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:spacer width="22" height="10" />
							<h:selectOneMenu id="basicDetails_enquirySegment"
								value="#{rfqSubmissionMB.rfqHeader.enquirySegment}"
								style="width:100px">
								<f:selectItems
									value="#{rfqSubmissionMB.enquirySegmentSelectList}" />
							</h:selectOneMenu>
							<p:spacer width="10" height="10" />
							<h:outputText value="#{dbResource['wq.label.projectName']}:"
								style="padding:0px 3px 0px 3px" />
							<p:autoComplete id="basicDetails_projectName" queryDelay="300"
								cache="true" cacheTimeout="30000" minQueryLength="5"
								maxResults="10" value="#{rfqSubmissionMB.rfqHeader.projectName}"
								dropdown="true"
								completeMethod="#{rfqSubmissionMB.autoCompleteProjectNameList}">
							</p:autoComplete>
							<p:spacer width="15" height="10" />
							<h:outputText value="#{dbResource['wq.label.app']}:"
								style="padding:0px 3px 0px 3px;" />
							<p:autoComplete id="basicDetails_application" queryDelay="300"
								cache="true" cacheTimeout="30000" minQueryLength="5"
								value="#{rfqSubmissionMB.rfqHeader.application}" dropdown="true"
								completeMethod="#{rfqSubmissionMB.autoCompleteApplicationList}"
								size="38" scrollHeight="200" style="width:220px">
							</p:autoComplete>
							<p:spacer width="15" height="10" />
							<h:outputText value="#{dbResource['wq.label.designRegion']}:"
								style="padding:0px 3px 0px 3px" />
							<p:selectOneMenu id="basicDetails_designRegion"
								value="#{rfqSubmissionMB.rfqHeader.designRegion}"
								editable="false" style="width:100px">
								<f:selectItems value="#{rfqSubmissionMB.designRegionSelectList}" />
								<f:ajax event="change"
									listener="#{rfqSubmissionMB.rfqHeader.setDesignLocation('')}"
									render="basicDetails_designLocation" />
							</p:selectOneMenu>
							<p:spacer width="15" height="10" />
							<h:outputText value="#{dbResource['wq.label.designLoc']}:"
								style="padding:0px 3px 0px 3px" />
							<!-- <p:selectOneMenu id="basicDetails_designLocation" value="#{rfqSubmissionMB.rfqHeader.designLocation}" editable="true" style="width:200px">
								<f:selectItems value="#{rfqSubmissionMB.designLocationSelectList}" />
							</p:selectOneMenu> -->

							<p:autoComplete id="basicDetails_designLocation"
								value="#{rfqSubmissionMB.rfqHeader.designLocation}"
								editable="true" queryDelay="300" cache="true" minQueryLength="1"
								size="38" scrollHeight="200" autocomplete="true" dropdown="true"
								completeMethod="#{rfqSubmissionMB.designRegionOnchange}">
								<f:attribute name="region"
									value="#{rfqSubmissionMB.rfqHeader.designRegion}" />
							</p:autoComplete>
							<!--  
							<p:selectOneMenu id="basicDetails_application" value="#{rfqSubmissionMB.rfqHeader.application}" editable="true">
								<f:selectItems value="#{rfqSubmissionMB.applicationSelectList}" />
							</p:selectOneMenu>
                            -->
						</h:panelGrid>

						<h:panelGrid columns="25" cellpadding="0"
							style="margin-top:8px; padding:4px 3px 4px 3px;">


							<h:outputText value="#{dbResource['wq.label.ppSchedule']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:spacer width="12" height="10" />
							<h:selectOneMenu id="basicDetails_ppSchedule"
								value="#{rfqSubmissionMB.rfqHeader.ppSchedule}">
								<f:selectItems value="#{rfqSubmissionMB.ppScheduleSelectList}" />
							</h:selectOneMenu>

							<p:spacer width="15" height="10" />
							<h:outputText value="#{dbResource['wq.label.MpSchedule']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<h:selectOneMenu id="basicDetails_mpSchedule"
								value="#{rfqSubmissionMB.rfqHeader.mpSchedule}">
								<f:selectItems value="#{rfqSubmissionMB.mpScheduleSelectList}" />
							</h:selectOneMenu>
							<p:spacer width="15" height="10" />
							<h:outputText value="#{dbResource['wq.label.QuoteType']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value="&#042;"
								style="color:#ff0000; padding:0px 0px 0px 0px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<h:selectOneMenu id="basicDetails_quoteType"
								value="#{rfqSubmissionMB.rfqHeader.quoteType}">
								<f:selectItems value="#{rfqSubmissionMB.quoteTypeList}" />
							</h:selectOneMenu>
							<p:spacer width="15" height="10" />
							<h:outputText value="#{dbResource['wq.label.loa']}:"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<p:selectBooleanCheckbox value="#{rfqSubmissionMB.rfqHeader.loa}">
							</p:selectBooleanCheckbox>
							<p:spacer width="15" height="10" />
							<h:outputText value="#{dbResource['wq.label.DesignInCat']}"
								style="padding:0px 0px 0px 3px" />
							<h:outputText value="&#042;"
								style="color:#ff0000; padding:0px 0px 0px 0px" />
							<h:outputText value=":" style="padding:0px 3px 0px 0px" />
							<h:selectOneMenu id="basicDetails_designInCat"
								value="#{rfqSubmissionMB.rfqHeader.designInCat}">
								<f:selectItems value="#{rfqSubmissionMB.designInCatList}" />
							</h:selectOneMenu>
							<p:spacer width="30" height="10" />
							<h:outputText value="#{dbResource['wq.label.refExchCurr']}: "
								escape="false" />
							<h:selectOneMenu id="header_Rfq_Currency"
								value="#{rfqSubmissionMB.rfqHeader.rfqCurr}">
								<f:selectItems value="#{rfqSubmissionMB.exCurrencyList}" />
								<p:ajax event="change"
											update=":form_rfqSubmission:header_DollarLink"
											listener="#{rfqSubmissionMB.updateRfqCurr}" />
							</h:selectOneMenu>
						</h:panelGrid>

						<h:panelGrid columns="15" cellpadding="0"
							style="padding:4px 3px 4px 3px;">

							<h:outputText value="#{dbResource['wq.label.reqRef']}:"
								style="padding:0px 3px 0px 3px" />
							<p:inputText value="#{rfqSubmissionMB.rfqHeader.yourReference}" />
							<p:spacer width="10" height="10" />
