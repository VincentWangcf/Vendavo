<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3c.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui">
	
<f:view contentType="text/html">	
    <h:head>
        <f:facet name="first">
            <meta http-equiv="X-UA-Compatible" content="IE=edge"  />
            <meta content='text/html; charset=UTF-8' http-equiv="Content-Type"/>
			<meta http-equiv="Cache-Control" Content="no-cache"/>
			<meta http-equiv="Pragma" Content="no-cache"/>
			<meta http-equiv="Expires" Content="0"/>            
            <title>Working Platform Page</title>
        </f:facet>    
    </h:head>
	<h:body>

	<h:form id="form_pendinglist" enctype="multipart/form-data">
			<p:panel header="Working Platform" id="pendinglist">
			<p:growl id="workingPlatformGrowl" sticky="true" showDetail="true"/> 
			<p:defaultCommand target=":form_pendinglist:dummy" />  
		    <p:dataTable	widgetVar="datatable_pendinglist_var"
		    				var="rfq"
		    				value="#{workingPlatformMB.workingPlatformItems}" 
		    				rowKey="#{rfq.id}" 
							selection="#{workingPlatformMB.selectedWorkingPlatformItems}"		    				
		                 	filteredValue="#{workingPlatformMB.filteredWorkingPlatformItems}"
		                 	id="datatable_pendinglist"
		    				rowIndexVar="rowIndex"
		    				paginator="true" 
		    				rows="5"
		    				rowsPerPageTemplate="5,10,15,20,25"
		                 	paginatorPosition="top"						    				
		                 	editable="true"
		                 	editMode="cell"    	
			                scrollable="true">		    				
						
				<p:ajax event="rowSelect" listener="#{workingPlatformMB.rowSelectupdate}"
							update=":form_rfqdetailreference:panelgrid_rfqdetailreference,
									:form_pos:datatable_posHistoryList,
        							:form_quotationHistory:quoteHistorySearch,
        							:form_systemQuotation:vendorQuoteHistorySearch"/>									
																								 				
				<p:column selectionMode="multiple" styleClass="column_validRfq"/>
               
		        <p:column headerText="Form#" sortBy="#{rfq.quote.formNumber}" filterMatchMode="contains" filterBy="#{rfq.quote.formNumber}" styleClass="column_formNumber readOnlyCell">
		        	#{rfq.quote.formNumber}
		        </p:column>     
		
		        <p:column headerText="Avnet Quote#" sortBy="#{rfq.alternativeQuoteNumber}" filterMatchMode="contains" filterBy="#{rfq.alternativeQuoteNumber}" styleClass="column_rfqCode readOnlyCell">
					#{rfq.alternativeQuoteNumber}
		        </p:column>

		        <p:column headerText="Quote&lt;br/&gt;Stage" sortBy="#{rfq.stage}" filterMatchMode="contains" filterBy="#{rfq.stage}" styleClass="column_rfqCode readOnlyCell">
					#{rfq.stage}   
		        </p:column>

		        <p:column headerText="Pending&lt;br/&gt;Day" sortBy="#{rfq.pendingDay}" filterMatchMode="contains" filterBy="#{rfq.pendingDay}" styleClass="column_icon readOnlyCell">
					#{rfq.pendingDay} 
		        </p:column>
		             	        
		        <p:column headerText="RFQ&lt;br/&gt;Status" sortBy="#{rfq.status}" filterMatchMode="contains" filterBy="#{rfq.status}" styleClass="column_rfqStatus readOnlyCell">
					#{rfq.status}   
		        </p:column>  

		        <p:column headerText="Valid&lt;br/&gt;RFQ" sortBy="#{rfq.validFlagStr}" filterMatchMode="contains" filterBy="#{rfq.validFlagStr}" styleClass="column_icon readOnlyCell">
					<h:outputText value="#{rfq.validFlag?'Yes':'No'}" />  
		        </p:column> 
		        		                
		        <p:column headerText="MFR" sortBy="#{rfq.requestedMaterial.manufacturer.name}" filterMatchMode="contains" filterBy="#{rfq.requestedMaterial.manufacturer.name}" styleClass="column_mfr_wp readOnlyCell">
					#{rfq.requestedMaterial.manufacturer.name}       
		        </p:column>
		              
				<p:column headerText="Quote type" sortBy="#{rfq.quoteItem.quoteType}" filterMatchMode="contains" filterBy="#{rfq.quoteItem.quoteType}" styleClass="column_quoteType readOnlyCell">
					#{rfq.quoteItem.quoteType}   
		        </p:column>    
		        <p:column headerText="Sold&lt;br/&gt;To Party" sortBy="#{rfq.soldToCustomerFullName}" filterMatchMode="contains" filterBy="#{rfq.soldToCustomerFullName}" styleClass="column_soldToCustomerName readOnlyCell">
					#{rfq.soldToCustomerFullName}
		        </p:column>
		
		        <p:column headerText="End&lt;br/&gt;Customer" sortBy="#{rfq.endCustomerFullName}" filterMatchMode="contains" filterBy="#{rfq.endCustomerFullName}" styleClass="column_endCustomerName readOnlyCell">
					#{rfq.endCustomerFullName}
		        </p:column>
		        
		        <p:column headerText="Requested&lt;br/&gt;P/N" sortBy="#{rfq.requestedMaterial.fullMfrPartNumber}" filterMatchMode="contains" filterBy="#{rfq.requestedMaterial.fullMfrPartNumber}" styleClass="column_fullMfrPart readOnlyCell">
					<h:outputText id="requiredPartNumber" value="#{rfq.requestedMaterial.fullMfrPartNumber}" />
		        </p:column>
		
		        <p:column headerText="Required&lt;br/&gt;Qty" sortBy="#{rfq.requestedQty}" filterMatchMode="contains" filterBy="#{rfq.requestedQty}" styleClass="column_requiredQty readOnlyCell">
					#{workingPlatformMB.convertNumber(rfq.requestedQty)}
		        </p:column>
		
		        <p:column headerText="EAU" sortBy="#{rfq.eau}" filterMatchMode="contains" filterBy="#{rfq.eau}" styleClass="column_requiredQty readOnlyCell">
					#{workingPlatformMB.convertNumber(rfq.eau)}
		        </p:column>
		
		        <p:column headerText="Target&lt;br/&gt;Resale" sortBy="#{rfq.targetResale}" filterMatchMode="contains" filterBy="#{rfq.targetResale}" styleClass="column_targetResale readOnlyCell">
			        <h:outputText value="#{rfq.targetResale}" rendered="#{rfq.targetResale != 0.0}">
			    		<f:convertNumber minFractionDigits="2" maxFractionDigits="5" />	
			        </h:outputText>
		        </p:column>
		                                                        
		        <p:column headerText="T/P&lt;br/&gt;Margin %" styleClass="column_targetPriceMargin readOnlyCell">
			        <h:outputText id="p_targetMargin" value="#{rfq.targetMarginStr}">
			        	<f:convertNumber minFractionDigits="2" maxFractionDigits="2"/>			        
			        </h:outputText>
		        </p:column>  
		                             
		        <p:column headerText="Quoted P/N" sortBy="#{rfq.quotedMaterial.fullMfrPartNumber}" filterMatchMode="contains" filterBy="#{rfq.quotedMaterial.fullMfrPartNumber}" styleClass="column_quotedPartNumber_wp tdmandatoryvalue">
                	<p:autoComplete id="p_quotedPartNumber" queryDelay="500" cache="true" cacheTimeout="30000" minQueryLength="5" maxResults="10" forceSelection="true" value="#{rfq.quotedMaterial.fullMfrPartNumber}" completeMethod="#{workingPlatformMB.autoCompletePartList}" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
						<p:ajax event="itemSelect" listener="#{workingPlatformMB.changeQuotedPartNumber}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>							
					</p:autoComplete>					                    
		        </p:column> 
		        
		        <p:column headerText="Other P/N&lt;br/&gt;Search" styleClass="column_icon readOnlyCell">
		        	<p:commandLink id="partNumberSearch_magnifier" actionListener="#{workingPlatformMB.searchForMaterialPopup}" update=":form_pendinglist:alternatePartSearch_panel" oncomplete="validation_dialog.show()" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />		        	
						<f:attribute name="targetRowIndex" value="#{rowIndex}" />		        	
		        		<h:graphicImage url="./../resources/images/icon_magnifier.png" styleClass="iconLink"/>				        	
		        	</p:commandLink> 				        
				</p:column>	
		                		        
		        <p:column headerText="Quoted&lt;br/&gt;Margin %" sortBy="#{rfq.resaleMarginStr}" filterMatchMode="contains" filterBy="#{rfq.resaleMarginStr}" styleClass="column_quotedMargin tdmandatoryvalue">
					<p:inputText id="p_quotedMargin" value="#{rfq.resaleMarginStr}" styleClass="mandatory_input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">		        														
						<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
						<p:ajax event="change" listener="#{workingPlatformMB.updateQuotedMargin}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>						
					</p:inputText>		        
		        </p:column> 
		                                                
		        <p:column headerText="Quoted&lt;br/&gt;Resale" sortBy="#{rfq.quotedPriceStr}" filterMatchMode="contains" filterBy="#{rfq.quotedPriceStr}" styleClass="column_quotedPrice tdmandatoryvalue">
					<p:inputText id="p_quotedPrice" value="#{rfq.quotedPriceStr}" styleClass="mandatory_input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">														
						<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
						<p:ajax event="change" listener="#{workingPlatformMB.updateQuotedPrice}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>									
					</p:inputText>       
		        </p:column>        
		       
		        <p:column headerText="Cost" sortBy="#{rfq.costStr}" filterMatchMode="contains" filterBy="#{rfq.costStr}" styleClass="column_cost tdmandatoryvalue">
					<p:inputText id="p_cost" value="#{rfq.costStr}" styleClass="mandatory_input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
						<p:ajax event="change" listener="#{workingPlatformMB.updateCost}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>							
					</p:inputText>
		        </p:column>
		       
		        <p:column headerText="Cost&lt;br/&gt;Indicator" sortBy="#{rfq.costIndicator}" filterMatchMode="contains" filterBy="#{rfq.costIndicator}" styleClass="column_costIndicator tdmandatoryvalue">
	    			<h:selectOneMenu id="p_costIndicator" value="#{rfq.costIndicator}" effect="size" onselect="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});">
						<f:selectItems value="#{workingPlatformMB.convertCostIndicatorToSelectItem()}" />
						<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
						<p:ajax event="change" listener="#{workingPlatformMB.switchCostIndicator}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>							    
					</h:selectOneMenu>					
		        </p:column> 
		        
		        <p:column headerText="Cost Indicator Search" styleClass="column_icon readOnlyCell">
		        	<p:commandLink actionListener="#{workingPlatformMB.searchForPricerPopup}" update=":form_pendinglist:costIndicator_panel" oncomplete="costIndicator_dialog.show()" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />			        	
		        		<h:graphicImage url="./../resources/images/icon_magnifier.png" styleClass="iconLink"/>				        	
		        	</p:commandLink> 				        
				</p:column>			         
		               	        
		        <p:column headerText="Qty&lt;br/&gt;Indicator" sortBy="#{rfq.qtyIndicator}" filterMatchMode="contains" filterBy="#{rfq.qtyIndicator}" styleClass="column_qtyIndicator tdmandatoryvalue">
	    			<h:selectOneMenu id="p_qtyIndicator" value="#{rfq.qtyIndicator}" effect="size" onselect="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});">
						<f:selectItems value="#{workingPlatformMB.qtyIndicatorOptions}" />
						<f:attribute name="targetQuoteItem" value="#{rfq}" />	                            
						<p:ajax event="change" listener="#{workingPlatformMB.updateQtyIndicator}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>	
					</h:selectOneMenu>					        
		        </p:column>  
		        
		        <p:column headerText="Quoted&lt;br/&gt;Qty" sortBy="#{rfq.quotedQtyStr}" filterMatchMode="contains" filterBy="#{rfq.quotedQtyStr}" styleClass="column_quotedQty tdmandatoryvalue">
					<p:inputText id="p_quotedQty" value="#{rfq.quotedQtyStr}" styleClass="mandatory_input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />						
						<p:ajax event="change" listener="#{workingPlatformMB.updateQuotedQty}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>							
					</p:inputText>        
		        </p:column>  
		                
		        <p:column headerText="Multi&lt;br/&gt;Usage" sortBy="#{rfq.multiUsageFlagStr}" filterMatchMode="contains" filterBy="#{rfq.multiUsageFlagStr}" styleClass="column_multiUsage editCell">
	    			<h:selectOneMenu value="#{rfq.multiUsageFlagStr}" effect="size" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});">											
			            <f:selectItem itemLabel="Yes" itemValue="Yes" />  
			            <f:selectItem itemLabel="No" itemValue="No" />  			            
					</h:selectOneMenu>					
		        </p:column>    
	        
		        <p:column headerText="Quantity Break"  styleClass="column_quantityBreak readOnlyCell">
					<h:outputText value="#{rfq.requestedMaterial.programMaterial}" converter="webquote.commodity.Oqmsp" escape="false" />
				</p:column>					        	
					        	
					        		        
		        <p:column headerText="QC External&lt;br/&gt;Comment" styleClass="column_attachmentId editCell">
		        	<p:commandLink oncomplete="qc_external_comment_dialog.show()" update=":form_pendinglist:qc_external_comment_panel" actionListener="#{workingPlatformMB.updateSelectedQuoteItem}" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />	
						<f:attribute name="targetRowIndex" value="#{rowIndex}" />																				    		        	
		        		<h:graphicImage url="./../resources/images/icon_info.png" styleClass="iconLink" rendered="#{!workingPlatformMB.isEmpty(rfq.aqcc)}"/>
		        		<h:graphicImage url="./../resources/images/icon_info_empty.png" styleClass="iconLink" rendered="#{workingPlatformMB.isEmpty(rfq.aqcc)}"/>
		        	</p:commandLink> 					        		        	
		        </p:column>            
	
		        <p:column headerText="QC Internal&lt;br/&gt;Comment" styleClass="column_attachmentId editCell">
		        	<p:commandLink oncomplete="qc_internal_comment_dialog.show()" update=":form_pendinglist:qc_internal_comment_panel" actionListener="#{workingPlatformMB.updateSelectedQuoteItem}" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />	
						<f:attribute name="targetRowIndex" value="#{rowIndex}" />																				    		        	
		        		<h:graphicImage url="./../resources/images/icon_info.png" styleClass="iconLink" rendered="#{!workingPlatformMB.isEmpty(rfq.internalComment)}"/>
		        		<h:graphicImage url="./../resources/images/icon_info_empty.png" styleClass="iconLink" rendered="#{workingPlatformMB.isEmpty(rfq.internalComment)}"/>
		        	</p:commandLink> 					        		        	
		        </p:column>            
						        
		        <p:column headerText="Lead&lt;br/&gt;Time" sortBy="#{rfq.leadTime}" filterMatchMode="contains" filterBy="#{rfq.leadTime}" styleClass="column_leadTime tdmandatoryvalue">
					<p:inputText id="p_leadTime" value="#{rfq.leadTime}" styleClass="mandatory_input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">				
					</p:inputText>    
		        </p:column>
		                                        
		        <p:column headerText="MPQ" sortBy="#{rfq.mpqStr}" filterMatchMode="contains" filterBy="#{rfq.mpqStr}" styleClass="column_mpq tdmandatoryvalue">
					<p:inputText id="p_mpq" value="#{rfq.mpqStr}" styleClass="mandatory_input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />																			    
						<p:ajax event="change" listener="#{workingPlatformMB.updatePmoq}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>					
					</p:inputText>      
		        </p:column>
		        
		        <p:column headerText="MOQ" sortBy="#{rfq.moqStr}" filterMatchMode="contains" filterBy="#{rfq.moqStr}" styleClass="column_moq tdmandatoryvalue">
					<p:inputText id="p_moq" value="#{rfq.moqStr}" styleClass="mandatory_input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />																			    
						<p:ajax event="change" listener="#{workingPlatformMB.updatePmoq}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>						
					</p:inputText>    
		        </p:column>   
		        
		        <p:column headerText="MOV" sortBy="#{rfq.movStr}" filterMatchMode="contains" filterBy="#{rfq.movStr}" styleClass="column_mov tdmandatoryvalue">
					<p:inputText id="p_mov" value="#{rfq.movStr}" styleClass="mandatory_input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />																			    
						<p:ajax event="change" listener="#{workingPlatformMB.updatePmoq}"
										update="p_mpq,
											    p_moq,
											    p_mov,
											    p_leadTime,
											    p_cost,
											    p_quotedMargin,
											    p_quotedPrice,
											    p_quotedQty,
											    p_validity,
											    p_shipmentValidity,
											    p_targetMargin,
											    p_costIndicator,
											    p_pmoq"/>																				    								
					</p:inputText>    
		        </p:column>                      
		                           
		        <p:column headerText="Price&lt;br/&gt;Validity" sortBy="#{rfq.priceValidity}" filterMatchMode="contains" filterBy="#{rfq.priceValidity}" styleClass="column_validity tdmandatoryvalue">
					<p:inputText id="p_validity" value="#{rfq.priceValidity}" styleClass="mandatory_input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">											
					</p:inputText>         
		        </p:column>   
		        
		        <p:column headerText="Shipment&lt;br/&gt;Validity" sortBy="#{rfq.shipmentValidity}" filterMatchMode="contains" filterBy="#{rfq.shipmentValidity}" styleClass="column_shipmentValidity editCell">
            		<p:inputText id="p_shipmentValidity" value="#{rfq.shipmentValidityStr}" styleClass="input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">											
					</p:inputText>						     
		        </p:column>                                                                              
		        
		        <p:column headerText="MFR Quote#" sortBy="#{rfq.vendorQuoteNumber}" filterMatchMode="contains" filterBy="#{rfq.vendorQuoteNumber}" styleClass="column_vendorQuoteNumber editCell">
					<p:inputText value="#{rfq.vendorQuoteNumber}" styleClass="input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">											
					</p:inputText>						
		        </p:column> 
		        
		        <p:column headerText="MFR Debit#" sortBy="#{rfq.vendorDebitNumber}" filterMatchMode="contains" filterBy="#{rfq.vendorDebitNumber}" styleClass="column_vendorDebitNumber editCell">
					<p:inputText value="#{rfq.vendorDebitNumber}" styleClass="input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">											
					</p:inputText>
		        </p:column>
		        
		        <p:column headerText="MFR Quote Qty" sortBy="#{rfq.vendorQuoteQtyStr}" filterMatchMode="contains" filterBy="#{rfq.vendorQuoteQtyStr}" styleClass="column_vendorQuoteQty editCell">
					<p:inputText value="#{rfq.vendorQuoteQtyStr}" styleClass="input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">											
					</p:inputText>    
		        </p:column> 
		        	        
		        <p:column headerText="Rescheduling&lt;br/&gt;Window" sortBy="#{rfq.rescheduleWindowStr}" filterMatchMode="contains" filterBy="#{rfq.rescheduleWindowStr}" styleClass="column_reschedulingWindow editCell">
					<p:inputText value="#{rfq.rescheduleWindowStr}" styleClass="input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">											
					</p:inputText>						    
		        </p:column>        
		        
		        <p:column headerText="Cancellation&lt;br/&gt;Window" sortBy="#{rfq.cancellationWindowStr}" filterMatchMode="contains" filterBy="#{rfq.cancellationWindowStr}" styleClass="column_cancellationWindow editCell">
					<p:inputText value="#{rfq.cancellationWindowStr}" styleClass="input_text" onclick="checkUpdateDetailPanel(#{rfq.id}, #{rowIndex});" onkeyup="checkEnterKey(event, this)">											
					</p:inputText>						  
		        </p:column>          		          
		        		
		        <p:column headerText="Quotation&lt;br/&gt;T&amp;C" styleClass="column_attachmentId editCell">
