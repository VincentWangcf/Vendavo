<ui:composition template="/templates/MasterLayoutAutoFit.xhtml"
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:avnet="http://emasia.avnet.com"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui"
	xmlns:c="http://java.sun.com/jsp/jstl/core">
	<ui:define name="windowTitle">
		#{dbResource['wq.title.webQuoteVersion']}
	</ui:define>
	<f:metadata>         
	   
	</f:metadata>
	<ui:define name="content"> 
	
			    <p:growl id="growl_submit" autoUpdate="true" showDetail="true" escape="#{growlPropertyMB.escape}" showSummary="#{growlPropertyMB.showSummary}"  sticky="#{growlPropertyMB.sticky}" />
			    <h:form id="form_submit" enctype="multipart/form-data" onkeypress="if (event.keyCode == 13) { document.getElementById('form_submit:basicDetails_soldToCustomerNumber').blur(); document.getElementById('form_submit:basicDetails_endCustomerNumber').blur();return false; }">
	            <!-- <p:commandLink id="asd" value="asdas">
	            	<avnet:convertBreakList2   target="asd"  targetField="das" />
	            </p:commandLink> -->
	             <p:column>
					 <h:outputText id="systemInfo" value="#{progRfqSubmit.systemInfo}" /> 
			     </p:column>
				<p:panel id="submissionHeaderPanel" header="#{dbResource['wq.header.headerLevel']}" style="width:100%;" toggleable="true">
				  <p:messages id="messages_submit" for="messages_submit_target" autoUpdate="true" closable="true" showDetail="true"  escape="false" /> 
				    <p:panelGrid style="width:100%"> 
				     <p:row>
				      	<p:column>
				      		<h:outputText value="#{dbResource['wq.label.region']}:"  escape="false" />
							<h:selectOneMenu id="region"  value="#{progRfqSubmitMB.region}" style="width:110px;">
								<f:selectItems value="#{progRfqSubmitMB.regionList}" />
								<p:ajax event="change" update=":menu_form:shoppingCartInfoLink,form_submit"
									listener="#{progRfqSubmitMB.regionChanged}"
									oncomplete="stop();" global="false" onstart="start();"
									onerror="stopWithError();" />
							</h:selectOneMenu>
				      	</p:column>
					 </p:row>
					 <c:choose> 
					 	<c:when test="#{!progRfqSubmitMB.salesRole}">
					 		 <p:row>  
				            <p:column>
					            <h:outputText value="#{dbResource['wq.label.salesmanEmpCode']}" style="padding:0px 0px 0px 3px"/>
								<h:outputText value="&#042;" style="color:#ff0000; padding:0px 0px 0px 0px"/>
								<h:outputText value=":" style="padding:0px 3px 0px 0px"/>
								<p:autoComplete id="salesEmployeeNumber" queryDelay="300" cache="false" cacheTimeout="30000" minQueryLength="3" maxResults="10" value="#{progRfqSubmitMB.quote.sales.employeeId}" 
									completeMethod="#{progRfqSubmitMB.autoCompleteSalesNumber}" >							
									<p:ajax event="itemSelect" update=":growl_submit,:form_submit:salesEmployeeName,:form_submit:salesEmployeeNumber,:form_submit:salesTeam,
											:form_submit:uploadBtn,
											:form_submit:basicDetails_soldToCustomerNumber,
											:form_submit:soldToCustSearch,
											:form_submit:crtNewSoldToCustmr,
											:form_submit:basicDetails_endCustomerNumber,
											:form_submit:endCustSearch,
											:form_submit:crtNewEndCust,
											:form_submit:submissionTable" listener="#{progRfqSubmitMB.updateSalesName}"  />															
								</p:autoComplete>
								<h:outputText value="#{dbResource['wq.label.salesman']}" style="padding:0px 0px 0px 3px"/>
								<h:outputText value="&#042;" style="color:#ff0000; padding:0px 0px 0px 0px"/>
								<h:outputText value=":" style="padding:0px 3px 0px 0px"/>							
								<p:autoComplete id="salesEmployeeName" queryDelay="300" cache="true" cacheTimeout="30000" minQueryLength="5" maxResults="10" value="#{progRfqSubmitMB.quote.sales.name}" 
									completeMethod="#{progRfqSubmitMB.autoCompleteSalesName}">								
									<p:ajax event="itemSelect" update=":growl_submit,:form_submit:salesEmployeeNumber,:form_submit:salesEmployeeName,:form_submit:salesTeam,:form_submit:uploadBtn,
											:form_submit:uploadBtn,
											:form_submit:basicDetails_soldToCustomerNumber,
											:form_submit:soldToCustSearch,
											:form_submit:crtNewSoldToCustmr,
											:form_submit:basicDetails_endCustomerNumber,
											:form_submit:endCustSearch,
											:form_submit:crtNewEndCust,
											:form_submit:submissionTable" listener="#{progRfqSubmitMB.updateSalesNumber}"  />															
								</p:autoComplete>
				                <h:outputText value="#{dbResource['wq.label.team']}:" style="padding:0px 3px 0px 3px"/>
							    <p:inputText id="salesTeam" value="#{progRfqSubmitMB.quote.team.name}" styleClass="input_text_half" readonly="true"  style="width:250px !important;color: black;background-color: #dddddd;"/>

							
				                
				                
				                <ui:remove>
				                <h:outputText value="#{dbResource['wq.label.salesmanName']}" style="padding:0px 3px 0px 3px"/> 
				                <h:outputText value="&#042;" style="color:#ff0000; padding:0px 0px 0px 0px"/>
				                <h:outputText value=":" style="padding:0px 3px 0px 0px"/>	
						    	<p:selectOneMenu id="salesmanName" value="#{progRfqSubmitMB.quote.sales.name}" style="width:130px;" >
						    	     <f:selectItem itemValue="" itemLabel="-Select-" /> 
								     <f:selectItems value="#{progRfqSubmitMB.salesList}" var="sales" itemValue="#{sales.name}" itemLabel="#{sales.name}"/>
								     <p:ajax event="change"  update=":form_submit:employeeId" listener="#{progRfqSubmitMB.updateSalesman}" />
							    </p:selectOneMenu>	
                                <p:spacer width="30" height="10" /> 
						    	<h:outputText for="employeeId" value="#{dbResource['wq.label.EmpID']} : " />
						    	<h:outputText id="employeeId"  value="#{progRfqSubmitMB.quote.sales.employeeId}" />
						    	</ui:remove>
						    </p:column>
					    	</p:row>
					 	</c:when>
					 	<c:otherwise>
					 	 <p:row>  
				            <p:column>
					 		 <h:outputText value="#{dbResource['wq.label.salesmanEmpCode']}" style="padding:0px 0px 0px 3px"/>
							<h:outputText value="&#042;" style="color:#ff0000; padding:0px 0px 0px 0px"/>
							<h:outputText value=":" style="padding:0px 3px 0px 0px"/>
							<p:inputText id="salesEmployeeNumber" value="#{progRfqSubmitMB.quote.sales.employeeId}" styleClass="input_text_half" readonly="true"  style="height:20px;width:100px !important;color: black;background-color: #dddddd;"/>
							 
							<h:outputText value="#{dbResource['wq.label.salesman']}" style="padding:0px 0px 0px 3px"/>
							<h:outputText value="&#042;" style="color:#ff0000; padding:0px 0px 0px 0px"/>
							<h:outputText value=":" style="padding:0px 3px 0px 0px"/>
							<p:inputText id="salesEmployeeName" value="#{progRfqSubmitMB.quote.sales.name}" styleClass="input_text_half" readonly="true"  style="height:20px;width:150px !important;color: black;background-color: #dddddd;"/>							
							 
			                <h:outputText value="#{dbResource['wq.label.team']}:" style="padding:0px 3px 0px 3px"/>
						    <p:inputText id="salesTeam" value="#{progRfqSubmitMB.quote.team.name}" styleClass="input_text_half" readonly="true"  style="height:20px;width:200px !important;color: black;background-color: #dddddd;"/>
					 		</p:column>
					    	</p:row>
					 	</c:otherwise>
					 </c:choose>
					 
					 </p:panelGrid>
					
					<p:panelGrid style="width:100%;"> 
					        <p:row>  
					            <p:column>
					    	        <h:outputText value="#{dbResource['wq.label.SoldToCode']}" style="padding:0px 0px 0px 3px"/>
							    	<h:outputText value="&#042;" style="color:#ff0000; padding:0px 0px 0px 0px"/>
									<h:outputText value=":" style="padding:0px 3px 0px 0px"/>	
									<p:inputText id="basicDetails_soldToCustomerNumber"
										value="#{progRfqSubmitMB.quote.soldToCustomer.customerNumber}" disabled="#{progRfqSubmitMB.emptySalesMan()}" > 
										 <p:ajax event="valueChange" update=":growl_submit,:form_submit:messages_submit,:form_submit:basicDetails_soldToCustomerName" listener="#{progRfqSubmitMB.updateSoldToCustomerName}" />															
									</p:inputText> 
		                            <p:spacer width="10" height="10" /> 
									<h:outputText value="#{dbResource['wq.label.SoldToParty']}" style="padding:0px 0px 0px 3px"/>
									<h:outputText value="&#042;" style="color:#ff0000; padding:0px 0px 0px 0px"/>
									<h:outputText value=":" style="padding:0px 3px 0px 0px"/>
									<p:inputText id="basicDetails_soldToCustomerName"  value="#{progRfqSubmitMB.quote.soldToCustomer.customerFullName}" 
									      readonly="true" style="color: black;background-color: #dddddd;" >	
									      <p:tooltip id="toolForbasicDetails_soldToCustomerName" for="basicDetails_soldToCustomerName" value="#{progRfqSubmitMB.quote.soldToCustomer.customerFullName}"/> 									
									</p:inputText>
									<p:spacer width="10" height="10" /> 
									<p:commandLink id="basicDetails_soldToDelete" rendered="#{progRfqSubmitMB.deleteCustomer}">
								        <h:graphicImage url="./../resources/images/iconCross.gif" styleClass="iconLink"/>				        	
						        	</p:commandLink>	
						        	<p:commandLink id="soldToCustSearch" update=":form_customer_search:customerSearch_panel" action="#{progRfqSubmitMB.changeToSoldToCodeSearch}" oncomplete="PF('customer_dialog').show();stop();" global="false"
												onstart="start();" onerror="stopWithError();" disabled="#{progRfqSubmitMB.emptySalesMan()}">
						        		<h:graphicImage url="./../resources/images/icon_magnifier.png" styleClass="iconLink"/>		        	
						        	</p:commandLink> 	
									<p:spacer width="10" height="10" /> 																	
									<h:outputText value="" style="padding:0px 3px 0px 3px"/>
						        	<p:commandButton id="crtNewSoldToCustmr" value="#{dbResource['wq.label.crtNewCustmr']}"  disabled="#{!progRfqSubmitMB.createProspectiveCustomerButtonDisplay or progRfqSubmitMB.emptySalesMan()}" actionListener="#{progRfqSubmitMB.updateNewProspectiveCustomer}" oncomplete="PF('newProspectiveCustomer_dialog').show()" update="newProspectiveCustomer_dialog"/>  											
					    	       
					    	        <p:spacer width="30" height="10" /> 
					    	        <h:outputText value="#{dbResource['wq.label.EndCustomerCode']}" style="padding:0px 0px 0px 3px"/>
									<h:outputText value=":" style="padding:0px 3px 0px 0px"/>
										
									<ui:remove><p:inputText id="basicDetails_endCustomerNumber" 
									value="#{progRfqSubmitMB.quote.endCustomer.customerNumber}"  
									disabled="#{progRfqSubmitMB.emptySalesMan()}" > 
										<p:ajax event="blur" update=":growl_submit,:form_submit:basicDetails_endCustomerName,:form_submit:basicDetails_endCustomerNumber" listener="#{progRfqSubmitMB.updateEndCustomerName}"/>															
									</p:inputText>
									</ui:remove>
									<p:autoComplete id="basicDetails_endCustomerNumber"
										disabled="#{progRfqSubmitMB.emptySalesMan()}"
										queryDelay="300" cache="false" cacheTimeout="30000"
										minQueryLength="3" maxResults="10"
										forceSelection="true"
										emptyMessage="#{dbResource['wq.message.noRecordFound']}."
										value="#{progRfqSubmitMB.quote.endCustomer.customerNumber}" size="18"
										update=":growl_submit,:form_submit:basicDetails_endCustomerName,:form_submit:basicDetails_endCustomerNumber"
										completeMethod="#{progRfqSubmitMB.autoCompleteEndCustomerNumberLabelList}">
											<p:ajax event="itemSelect"
											update=":growl_submit,:form_submit:basicDetails_endCustomerName,:form_submit:basicDetails_endCustomerNumber"
											listener="#{progRfqSubmitMB.updateEndCustomerName}" />
											<p:ajax event="change" listener="#{progRfqSubmitMB.setNeedSave(true)}" /> 
									</p:autoComplete>
									<p:spacer width="15" height="10" /> 
									<h:outputText value="#{dbResource['wq.label.endCust']}" style="padding:0px 0px 0px 3px"/>
									<h:outputText value=":" style="padding:0px 3px 0px 0px"/>
									<p:inputText id="basicDetails_endCustomerName"  value="#{progRfqSubmitMB.quote.endCustomer.customerFullName}" 
									      readonly="true" style="color: black;background-color: #dddddd;">	
									      <p:tooltip id="toolForbasicDetails_endCustomerName" for="basicDetails_endCustomerName" value="#{progRfqSubmitMB.quote.endCustomer.customerFullName}"/> 									
									</p:inputText>
									<p:spacer width="10" height="10" /> 
									<p:commandLink id="basicDetails_endDelete" rendered="#{progRfqSubmitMB.deleteCustomer}">
								        <h:graphicImage url="./../resources/images/iconCross.gif" styleClass="iconLink"/>				        	
						        	</p:commandLink>	
						        	 
						        	<p:commandLink id="endCustSearch" update=":form_customer_search:customerSearch_panel" 
						        		action="#{progRfqSubmitMB.changeToEndCustomerCodeSearch}" oncomplete="PF('customer_dialog').show();stop();" global="false"
												onstart="start();" onerror="stopWithError();" disabled="#{progRfqSubmitMB.emptySalesMan()}">
					        			<h:graphicImage url="./../resources/images/icon_magnifier.png" styleClass="iconLink"/>		        	
						        	</p:commandLink>	
									<p:spacer width="10" height="10" /> 																	<!-- :form_customer_new -->
									<h:outputText value="" style="padding:0px 3px 0px 3px"/>
						        	<p:commandButton id="crtNewEndCust" value="#{dbResource['wq.label.crtEndCust']}" disabled="#{!progRfqSubmitMB.createProspectiveCustomerButtonDisplay or progRfqSubmitMB.emptySalesMan()}" actionListener="#{progRfqSubmitMB.updateNewProspectiveEndCustomer}" oncomplete="PF('newProspectiveCustomer_dialog').show()" update="newProspectiveCustomer_dialog"/>  											
						       </p:column>
						    </p:row>
						    
						    
						    <p:row> 
						        <p:column>
									<h:outputLabel for="remarksToCustomer" value="#{dbResource['wq.label.remarks']}" />
									<h:outputText value=":" style="padding:0px 3px 0px 0px"/>
									<p:inputTextarea  id="remarksToCustomer" value="#{progRfqSubmitMB.quote.remarks}"  rows="2" cols="35" >
										<p:ajax event="change"  listener="#{progRfqSubmitMB.remarksChange}"/>
									</p:inputTextarea>
									&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;		
								   <h:outputText value="#{dbResource['wq.label.refExchCurr']}:"  escape="false" />
									<h:selectOneMenu id="exChange_Currency" value="#{progRfqSubmitMB.rfqCurr}">							
				                        <f:selectItems value="#{progRfqSubmitMB.exCurrencyList}" />
									</h:selectOneMenu>
										
								</p:column>
							</p:row>
							<p:row> 
						        <p:column>
									<p:commandButton value="#{dbResource['wq.button.updateToItem']}" update=":form_submit:submissionTable"
						     			action="#{progRfqSubmitMB.updateSoldToAndEndCustomerToItem}"/>
								</p:column>
							</p:row>
					</p:panelGrid>
				</p:panel>
				<p:panel header="#{dbResource['wq.header.itemLevel']}"  id="submissionBody" position="center" style="margin:0 auto;width:100%;" toggleable="true">
					<p:contextMenu for="submissionTable" widgetVar="cMenu">      
					   <p:menuitem value="#{dbResource['wq.header.editCell']}" icon="ui-icon-search" onclick="PF('submissionTableWidget').showCellEditor();return false;"/>     
					   <p:menuitem value="#{dbResource['wq.header.hideMenu']}" icon="ui-icon-close" onclick="PF('cMenu').hide()"/>     
					</p:contextMenu> 
					<p:dataTable id="submissionTable" widgetVar="submissionTableWidget"
					         var="item"		
					 	     value="#{progRfqSubmitMB.quote.quoteItems}"
					 	     styleClass="submissionTable"
	                 	     rowStyleClass="#{item.error ? 'redRow' : null}"
			                 paginator="true" rows="25"
			                 paginatorTemplate="{CurrentPageReport}  {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
			                 rowsPerPageTemplate="25,50,100"
			                 paginatorPosition="bottom"
			                 rowIndexVar="rowIndex"
			                 emptyMessage="#{dbResource['wq.message.noRecordWidCriteria']}" 
			                 scrollable="true"
	                 	     scrollHeight="200"
	                 	     selection="#{progRfqSubmitMB.selectedQuoteItem}"
	                 	     editable="true" editMode="cell" 
	                 	      >
					         
					        <p:ajax event="cellEdit" listener="#{progRfqSubmitMB.onCellEdit}" update=":growl_submit" />  
					      
							<p:column headerText="#{dbResource['wq.label.no']}" id="sequenceNumber"  width="40">
								<h:outputText value="#{rowIndex+1}" /> 
							</p:column>
							 <p:column headerText="" id="delete"  width="40">
				                <p:commandLink id="deleteItem"  
									  action="#{progRfqSubmitMB.removeRfqItemAction}" update=":form_submit:submissionTable">
									 <h:graphicImage url="./../resources/images/iconCross.gif" styleClass="iconLink"/>
									 <f:setPropertyActionListener value="#{rowIndex}" target="#{progRfqSubmitMB.selectedRowIndex}" />
								</p:commandLink>
						     </p:column>
							<p:column headerText="#{dbResource['wq.label.mfr']}" id="mfr"  width="50" styleClass="tdmandatoryvalue readOnlyCell">
						        <h:outputText value="#{item.requestedMfr.name}" /> 
					        </p:column>
							<p:column headerText="#{dbResource['wq.label.reqPN']}" id="requestedPn" styleClass="tdmandatoryvalue readOnlyCell column_fullMfrPart">
						        <h:outputText value="#{item.requestedPartNumber}" /> 
					   		</p:column>
				            <p:column headerText="#{dbResource['wq.header.soldToCustomerCode']}&#042;" sortBy="#{item.soldToCustomer.customerNumber}"
				            	 styleClass="editCell tdmandatoryvalue column_soldToCustomerNumber" >
						        <p:cellEditor>  
							        <f:facet name="output">
							        	<h:outputText value="#{item.soldToCustomer.customerNumber}"/>
							        </f:facet>
							        <f:facet name="input">
						                <p:inputText style="width:80px" value="#{item.soldToCustomer.customerNumber}" id="soldToCustomerNumber_in" disabled="#{progRfqSubmitMB.emptySalesMan()}"  >
						                	 <p:ajax event="change" update=":growl_submit,submissionTable" listener="#{progRfqSubmitMB.updateSoldToCustomerNameInProgRfqItemList(rowIndex)}" />
						                </p:inputText>
							         </f:facet>
						          </p:cellEditor> 
					        </p:column>
					        
					         <p:column headerText="#{dbResource['wq.header.soldToCustomerName']}" width="200"  styleClass="readOnlyCell column_soldToCustomerChineseName" >
						        <h:outputText value="#{item.soldToCustomerFullName}"/>
					        </p:column> 
					        
					        <p:column headerText="#{dbResource['wq.label.EndCustomerCode']}" sortBy="#{item.endCustomer.customerNumber}" 
					         styleClass="column_endCustomerNumber  editCell" >
						        <p:cellEditor>  
							        <f:facet name="output">
						        		<h:outputText value="#{item.endCustomer.customerNumber}"/>
						        	</f:facet>
							         <f:facet name="input">
							            <p:autoComplete id="basicDetails_endCustomerNumber"
											disabled="#{progRfqSubmitMB.emptySalesMan()}"
											style="width:70px;margin-left:-0px"
											queryDelay="300" cache="false" cacheTimeout="30000"
											minQueryLength="3" maxResults="10"
											value="#{item.endCustomer.customerNumber}" size="10"
											forceSelection="true"
											emptyMessage="#{dbResource['wq.message.noRecordFound']}."
											update=":growl_submit,:form_submit:submissionTable"
											completeMethod="#{progRfqSubmitMB.autoEndCustomerNumberLabelListForItem}">
												<p:ajax event="itemSelect"
												update=":growl_submit,:form_submit:submissionTable"
												listener="#{progRfqSubmitMB.updateEndCustomerNameInProgRfqItemList(rowIndex)}" />
												<p:ajax event="change" listener="#{progRfqSubmitMB.setNeedSave(true)}" /> 
										</p:autoComplete>
							         </f:facet>
						          </p:cellEditor>
					        </p:column>  
					        
					         <p:column headerText="#{dbResource['wq.label.endCustName']}" sortBy="#{item.endCustomerFullName}" width="200" styleClass="readOnlyCell">
						        <h:outputText value="#{item.endCustomerFullName}"/>
					        </p:column>
					        <p:column headerText="#{dbResource['wq.label.MPQ']}" id="mpq"  width="80" styleClass="readOnlyCell column_mpq">
						        <h:outputText value="#{item.mpq}" > 
									<f:convertNumber  pattern="#,###"/>
								</h:outputText>
					        </p:column>
							<p:column headerText="#{dbResource['wq.label.moq']}" id="moq"   width="80" styleClass="readOnlyCell column_moq">
						        <h:outputText value="#{item.moq}" >
						        	<f:convertNumber  pattern="#,###" />
								</h:outputText> 
					        </p:column>
					        <p:column headerText="#{dbResource['wq.label.reqQty']}&#042;" id="requiredQty"   width="80" styleClass="editCell tdmandatoryvalue" >
						        <p:cellEditor>  
					                <f:facet name="output">
					                      <h:outputText id="requiredQtyProg_out" value="#{item.requestedQty}" >
							                      <f:convertNumber  pattern="#,###" />
										  </h:outputText>
					                </f:facet>  
					                <f:facet name="input">
					                		<p:inputText value="#{item.requestedQty}" style="width:65px" id="requiredQtyProg_in"  required="true" onkeyup="updateProgRequiredQty(#{rowIndex})">
					                        </p:inputText>
					                </f:facet>  
					            </p:cellEditor>
					        </p:column>
					        <p:column headerText="#{dbResource['wq.label.trgResale']}" id="targetResale"   width="80">
						         <p:cellEditor>  
					                <f:facet name="output">
						                 <h:outputText id="targetResaleProg_out" value="#{item.targetResale}" >
						                    	<f:convertNumber minFractionDigits="5"  pattern="#,##0.00000"  />
									     </h:outputText>
					                </f:facet>  
					                <f:facet name="input">
						                <p:inputText value="#{item.targetResale}" style="width:65px" id="targetResaleProg_in" onkeyup="updateProgTargetResale(#{rowIndex})">
						                </p:inputText>
						            </f:facet>  
					            </p:cellEditor> 
					        </p:column>
					        
					        <ui:remove>
						        <p:column headerText="#{dbResource['wq.label.finalPrice']}" id="finalPrice"   width="80">
							         <p:cellEditor>  
						                <f:facet name="output">
							                 <h:outputText id="finalPriceProg_out" value="#{item.finalQuotationPrice}" >
							                    	<f:convertNumber minFractionDigits="5"  pattern="#,##0.00000"  />
										     </h:outputText>
						                </f:facet> 
						                <f:facet name="input">
							                <p:inputText value="#{item.finalQuotationPrice}" id="finalPriceProg_in" onkeyup="updateProgFinalPrice(#{rowIndex})">
							                </p:inputText>
							            </f:facet>  
						            </p:cellEditor> 
						        </p:column>
					        </ui:remove>
					        
							<p:column headerText="#{dbResource['wq.message.orderQty']}"  width="110" styleClass="readOnlyCell">
						        <h:outputText id="orderQtys"  value="#{progRfqSubmitMB.getOrderQties(item)}"  escape="false">
									<avnet:convertBreakList targetField="CVT_BREAKINT" />
								</h:outputText>
					        </p:column>
					        
					 		<p:column headerText="#{dbResource['wq.label.salesCost']}" styleClass="readOnlyCell" width="75">
					            <h:outputText value="#{progRfqSubmitMB.getOrderQties(item)}"  escape="false">
					            	<avnet:convertBreakList targetField="CVT_SALESCOST" />
								</h:outputText>
					        </p:column>
					        
