<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3c.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui">
    <h:head>
        <f:facet name="first">
            <meta http-equiv="X-UA-Compatible"  content="IE=edge"/>
            <meta content='text/html; charset=UTF-8' http-equiv="Content-Type"/>
			<meta http-equiv="Cache-Control" Content="no-cache"/>
			<meta http-equiv="Pragma" Content="no-cache"/>
			<meta http-equiv="Expires" Content="0"/>            
            <title>#{dbResource['wq.label.workingPlateFormPage']}</title>
        </f:facet>    
    </h:head>
		<h:body>
	<p:accordionPanel id="ap_rfqdetailreference">
		<p:tab title="#{dbResource['wq.label.rfqDetails']}">
			<h:form id="form_rfqdetailreference">
			<p:growl id="rfq_detail_workingPlatformGrowl"  for="rfqDetailKey"  showDetail="true" sticky="#{growlPropertyMB.sticky}" autoUpdate="true" showSummary="#{growlPropertyMB.showSummary}" escape="#{growlPropertyMB.escape}"/> 			
			<p:defaultCommand target=":ap_rfqdetailreference:form_rfqdetailreference:dummyReference" />  			
			<p:panel id="panelgrid_rfqdetailreference" header=" " style="padding:0px 0px 0px 0px;">
				<p:growl id="workingPlatformRfqGrowl" showDetail="true" sticky="#{growlPropertyMB.sticky}" autoUpdate="true" showSummary="#{growlPropertyMB.showSummary}" escape="#{growlPropertyMB.escape}"/>		
			     <p:panelGrid styleClass="panelGrid" style="width:100%;padding:0px 0px 0px 0px;" widgetVar="rfqDialog">   				

			     	<p:row>  
						<p:column styleClass="tdtag" style="width:78px !important;">#{dbResource['wq.header.form']}#</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" style="width:130px" colspan="3">#{workingPlatformMB.selectedQuoteItem.quoteItem.quote.formNumber}</p:column>
						
						<p:column styleClass="tdtag" style="width:78px !important;">#{dbResource['wq.label.avnetQuote']}#</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" style="width:78px !important;">#{workingPlatformMB.selectedQuoteItem.quoteItem.alternativeQuoteNumber}</p:column>
									
						<p:column styleClass="tdtag" style="width:78px !important;">#{dbResource['wq.label.custmrType']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3" style="width:169px">#{workingPlatformMB.selectedQuoteItem.quoteItem.quote.customerType}</p:column>
						
						<p:column styleClass="tdtag" style="width:78px !important;">#{dbResource['wq.label.mfr']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3" style="width:78px">#{workingPlatformMB.selectedQuoteItem.quoteItem.requestedMfr.name}</p:column>
						
						<p:column styleClass="tdtag" style="width:78px !important;">#{dbResource['wq.label.reqPN']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" style="width:143px !important;">#{workingPlatformMB.selectedQuoteItem.requestedPartNumber}</p:column>
														
						<p:column styleClass="tdtag" style="width:78px !important;">#{dbResource['wq.label.reqQty']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" style="width:78px !important;">#{workingPlatformMB.convertNumber(workingPlatformMB.selectedQuoteItem.quoteItem.requestedQty)}</p:column>
													
						<p:column styleClass="tdtag" style="width:78px !important;">#{dbResource['wq.label.EAU']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" style="width:78px !important;">#{workingPlatformMB.convertNumber(workingPlatformMB.selectedQuoteItem.quoteItem.eau)}</p:column>							
					</p:row>
					
					<p:row>  			
						<p:column styleClass="tdtag">#{dbResource['wq.label.revertVersion']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3">#{workingPlatformMB.selectedQuoteItem.quoteItem.revertVersion}</p:column>			
						<p:column styleClass="tdtag">#{dbResource['wq.label.team']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all">#{workingPlatformMB.selectedQuoteItem.quoteItem.quote.team.name}</p:column>
						<p:column styleClass="tdtag">#{dbResource['wq.label.salesman']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3">#{workingPlatformMB.selectedQuoteItem.quoteItem.quote.sales.name}</p:column>
						<p:column styleClass="tdtag">#{dbResource['wq.label.validRFQ']}</p:column>						
						<p:column styleClass="tdvalue tdmandatoryvalue" style="width:20px !important;">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.validFlagStr}" />						
							<h:selectOneMenu rendered="#{workingPlatformMB.editMode}" id="wpInput_validFlag" value="#{workingPlatformMB.selectedQuoteItem.validFlagStr}" effect="size">
					            <f:selectItem itemLabel="Yes" itemValue="Yes" />  
			            		<f:selectItem itemLabel="No" itemValue="No" />  
								<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	                            
								<p:ajax event="change" listener="#{workingPlatformMB.triggerChangeFromTab}"/>						            									 
							</h:selectOneMenu>					
						</p:column>
						<p:column styleClass="tdtag" style="width:38px !important;">#{dbResource['wq.label.rfqStatus']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell" style="width:20px !important;">#{workingPlatformMB.selectedQuoteItem.quoteItem.status}</p:column>
						<p:column styleClass="tdtag">#{dbResource['wq.label.quotedPN']}</p:column>										
						<p:column styleClass="tdmandatoryvalue break-all">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.quotedPartNumber}" />
		                	<p:inputText id="d_quotedPartNumber" rendered="#{workingPlatformMB.editMode}" styleClass="inputInRFQDetailDataNumber tdmandatoryvalue" value="#{workingPlatformMB.selectedQuoteItem.quotedPartNumber}" onkeyup="checkEnterKey(event, this)" >
								<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />		        	
								<p:ajax event="change" listener="#{workingPlatformMB.changeQuotedPartNumber}"
														update="d_sapPartNumber,
																d_mpq,
															    d_moq,
															    d_mov,
															    d_leadTime,
															    d_cost,
															    d_quotedMargin,
															    d_quotedPrice,
															    d_quotedQty,
															    d_validity,
															    d_shipmentValidity,
															    d_targetMargin,
															    d_costIndicator,
															    d_minSell,
															    d_normSell,
															    d_priceListRemark1,
															    d_priceListRemark2
															    d_priceListRemark3,
															    d_priceListRemark4,
												p_ref_orderQtys_id,
												p_ref_minSellPrice_id,
												p_ref_salesCost_id,
												p_ref_suggestedResale_id,
											    d_salescosttype,
												d_salescost,
												d_suggestedResale,
															    d_authGp,
															    d_authGpReasonDesc,
															    d_authGpRemark,
															    d_drExpiryDate,
															    d_cancelWindow,
															    d_reschedulingWindow,
															    d_quotationEffective,															    
															    detail_quotationTc"/>
							</p:inputText>
							<p:commandLink rendered="#{workingPlatformMB.editMode}"
											global="false" onstart="start();" oncomplete="stop(); PF('validation_dialog').show();" onerror="stopWithError();" 
											actionListener="#{workingPlatformMB.searchForMaterialPopup}" update=":ap_rfqdetailreference:form_rfqdetailreference:rfq_detail_alternatePartSearch_panel">
								<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />		        	
								<f:attribute name="targetRowIndex" value="#{workingPlatformMB.rowIndex}" />		        	
				        		<h:graphicImage url="./../resources/images/icon_magnifier.png" styleClass="iconLink"/>				        	
				        	</p:commandLink> 
						</p:column>											
						<p:column styleClass="tdtag">#{dbResource['wq.label.qtyIndicator']}</p:column>
						<p:column styleClass="tdmandatoryvalue break-all">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.qtyIndicatorStr}" />						
							<h:selectOneMenu rendered="#{workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.qtyIndicatorStr}" effect="size">
								<f:selectItems value="#{workingPlatformMB.qtyIndicatorOptions}" />						 
							</h:selectOneMenu>					
						</p:column>	
						<p:column styleClass="tdtag">#{dbResource['wq.label.multiUsage']}</p:column>
						<p:column styleClass="tdvalue tdmandatoryvalue">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.multiUsageFlagStr}" />						
							<h:selectOneMenu rendered="#{workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.multiUsageFlagStr}" effect="size">
					            <f:selectItem itemLabel="Yes" itemValue="Yes" />  
			            		<f:selectItem itemLabel="No" itemValue="No" />  	
								<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	                            
								<p:ajax event="change" listener="#{workingPlatformMB.triggerChangeFromTab}"/>						            							 
							</h:selectOneMenu>					
						</p:column>
						
					</p:row>					

					<p:row>  			
						<p:column styleClass="tdtag">#{dbResource['wq.label.QuoteType']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell" colspan="3">
                            <h:outputText value="#{workingPlatformMB.selectedQuoteItem.quoteTypeStr}" />						
                        </p:column>	
						<p:column styleClass="tdtag">#{dbResource['wq.label.SoldToCode']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all">#{workingPlatformMB.selectedQuoteItem.quoteItem.quote.soldToCustomer.customerNumber}</p:column>							
						<p:column styleClass="tdtag">#{dbResource['wq.label.SoldToParty']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3">#{workingPlatformMB.selectedQuoteItem.soldToCustomerNameStr}</p:column>															
						<p:column styleClass="tdtag">#{dbResource['wq.label.resaleIndicator']}</p:column>
						<p:column styleClass="tdvalue tdmandatoryvalue break-all" colspan="3">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.resaleIndicatorStr}" />						
							<p:inputText rendered="#{workingPlatformMB.editMode}" styleClass="inputInRFQDetailDataText tdmandatoryvalue" value="#{workingPlatformMB.selectedQuoteItem.resaleIndicatorStr}" onkeyup="checkEnterKey(event, this)">
								<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	                            
								<p:ajax event="change" listener="#{workingPlatformMB.triggerChangeFromTab}"/>															
							</p:inputText>
						</p:column>
						<p:column styleClass="tdtag">#{dbResource['wq.label.bottomPrice']}</p:column>	
						<p:column styleClass="tdvalue readOnlyCell break-all">
							<h:outputText id="d_minSell" value="#{workingPlatformMB.selectedQuoteItem.bottomPriceStr}"/>
						</p:column>			
						<p:column styleClass="tdtag">#{dbResource['wq.label.minSellPrice']}</p:column>	
						<p:column styleClass="tdvalue readOnlyCell break-all">
							<h:outputText id="d_normSell" value="#{workingPlatformMB.selectedQuoteItem.minSellPriceStr}"/>
						</p:column>
						<p:column styleClass="tdtag">#{dbResource['wq.label.quotedQty']}</p:column>
						<p:column styleClass="editCell break-all">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.quotedQtyStr}" />						
							<p:inputText rendered="#{workingPlatformMB.editMode}" id="d_quotedQty" styleClass="optional_input_text inputInRFQDetailDataNumber" 
							value="#{workingPlatformMB.selectedQuoteItem.quotedQtyStr}" onclick="checkQtyIndicator()" onkeyup="checkEnterKey(event, this)"  onkeypress="return isInteger(event)">
								<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	                            
								<p:ajax event="change" listener="#{workingPlatformMB.triggerChangeFromTab}"/>								
							</p:inputText>
						</p:column>	
					</p:row>
					<p:row>
						<p:column styleClass="tdtag">#{dbResource['wq.label.SoldToParty']} (#{dbResource['wq.label.chinese']})</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3">#{workingPlatformMB.selectedQuoteItem.soldToChineseNameStr}</p:column>
						<p:column styleClass="tdtag">#{dbResource['wq.label.EndCustomerCode']}</p:column>
						<p:column styleClass="tdvalue editCell break-all">
						    <h:outputText id="out_endCustomerCode"  value="#{workingPlatformMB.selectedQuoteItem.endCustomerCodeStr}" />												
						</p:column>
						<p:column styleClass="tdtag">#{dbResource['wq.label.endCust']}</p:column>
						<p:column styleClass="tdvalue editCell break-all" colspan="3">
						    <h:outputText id="out_endCustomerName"  value="#{workingPlatformMB.selectedQuoteItem.endCustomerNameStr}" />				        
			        	<p:commandLink rendered="#{workingPlatformMB.editMode}" actionListener="#{workingPlatformMB.changeToEndCustomerCodeSearch}" update=":ap_rfqdetailreference:form_endCustomerSearch:rfq_details_end_customerSearch_panel" oncomplete="PF('rfq_details_end_customer_dialog').show();stop();" global="false" onstart="start();" onerror="stopWithError();">
				        	    <f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	        	
				        		<h:graphicImage url="./../resources/images/icon_magnifier.png" styleClass="iconLink"/>			        	
				        </p:commandLink>
				</p:column> 
						<p:column styleClass="tdtag">#{dbResource['wq.label.trgResale']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3">
					        <h:outputText value="#{workingPlatformMB.selectedQuoteItem.quoteItem.targetResale}">
					    		<f:convertNumber minFractionDigits="2" maxFractionDigits="5" />	
					        </h:outputText>
						</p:column>	
						<p:column styleClass="tdtag">#{dbResource['wq.label.qouteResale']}</p:column>
						<p:column styleClass="tdmandatoryvalue break-all">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.quotedPriceStr}" />						
							<p:inputText rendered="#{workingPlatformMB.editMode}"  disabled ="#{workingPlatformMB.selectedQuoteItem.disableEditableForSalesCost}" id="d_quotedPrice" styleClass="inputInRFQDetailDataNumber tdmandatoryvalue" 
							value="#{workingPlatformMB.selectedQuoteItem.quotedPriceStr}" onkeyup="checkEnterKey(event, this)" onkeypress="return isDouble(event, true)">								
								<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	                            
								<p:ajax event="change" listener="#{workingPlatformMB.updateQuotedPrice}"
												update="d_cost,
													    d_quotedMargin,
													    d_quotedPrice,
													    d_targetMargin"/>										
							</p:inputText>
						</p:column>												
						<p:column styleClass="tdtag">#{dbResource['wq.label.costIndicator']}</p:column>
						<p:column styleClass="tdmandatoryvalue break-all">
							<div style='width:100%'>
								<div style='width:80%'>
									<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.costIndicatorStr}" />								
					    			<h:selectOneMenu rendered="#{workingPlatformMB.editMode}" id="d_costIndicator" value="#{workingPlatformMB.selectedQuoteItem.costIndicatorStr}" effect="size">
										<f:selectItems value="#{workingPlatformMB.convertCostIndicatorToSelectItem()}" />											    			
										<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	                            
										<p:ajax event="change" listener="#{workingPlatformMB.switchCostIndicator}"
														update="d_mpq,
															    d_moq,
															    d_mov,
															    d_leadTime,
															    d_cost,
															    d_quotedMargin,
															    d_quotedPrice,
															    d_quotedQty,
															    d_validity,
															    d_shipmentValidity,
															    d_targetMargin,
															    d_costIndicator,
															    d_minSell,
															    d_normSell,
															    d_priceListRemark1,
															    d_priceListRemark2
															    d_priceListRemark3,
															    d_priceListRemark4,
												p_ref_orderQtys_id,
												p_ref_minSellPrice_id,
												p_ref_salesCost_id,
												p_ref_suggestedResale_id,
												d_salescosttype,
												d_salescost,
												d_suggestedResale,
															    d_authGp,
															    d_authGpReasonDesc,
															    d_authGpRemark,
															    d_drExpiryDate,	
															    d_quotationEffective,														    
															    detail_quotationTc"/>
									</h:selectOneMenu>	
								</div>							
								<div style='width:20%'>
						        	<p:commandLink rendered="#{workingPlatformMB.editMode}" actionListener="#{workingPlatformMB.searchForPricerPopup}" update=":form_pendinglist_dialog:costIndicator_panel" oncomplete="PF('costIndicator_dialog').show()">
										<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />			        	
						        		<h:graphicImage url="./../resources/images/icon_magnifier.png" styleClass="iconLink"/>				        	
						        	</p:commandLink>
						        </div>			        	
				        	</div> 						
						</p:column>	
						<p:column styleClass="tdtag">#{dbResource['wq.label.cost']}</p:column>
						<p:column styleClass="tdmandatoryvalue break-all">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.costStr}" />						
							<p:inputText rendered="#{workingPlatformMB.editMode}" id="d_cost" styleClass="inputInRFQDetailDataNumber tdmandatoryvalue" 
							value="#{workingPlatformMB.selectedQuoteItem.costStr}" onkeyup="checkEnterKey(event, this)" onkeypress="return isDouble(event, true)">
								<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	                            
								<p:ajax event="change" listener="#{workingPlatformMB.updateCost}"
												update="d_cost,
													    d_quotedMargin,
													    d_quotedPrice,
													    d_targetMargin"/>																
							</p:inputText>
						</p:column>														
					</p:row>
					
					
					<p:row>
					    <p:column styleClass="tdtag">#{dbResource['wq.label.customerGroupID']} #</p:column>
						<p:column styleClass="tdvalue editCell break-all" colspan="5">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.customerGroupId}" />												
							<p:inputText rendered="#{workingPlatformMB.editMode}" styleClass="optional_input_text" 
							value="#{workingPlatformMB.selectedQuoteItem.customerGroupId}" >
															
							</p:inputText>
						</p:column>	
		
					    
							
				        <p:column styleClass="tdtag">#{dbResource['wq.label.salesCostPart']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3">
					        <h:outputText value="#{workingPlatformMB.selectedQuoteItem.quoteItem.salesCostFlag =='true'?'Yes':'No'}">
					        </h:outputText>
						</p:column>	
					
					
					   <p:column styleClass="tdtag">#{dbResource['wq.label.salesCostType']} #</p:column>
							<p:column 
								styleClass="editCell break-all" colspan="3">
								<div style='width: 100%'>
									<h:outputText rendered="#{!workingPlatformMB.editMode}"
										value="#{workingPlatformMB.selectedQuoteItem.salesCostTypeName}" />
									<h:selectOneMenu  id="d_salescosttype" rendered="#{workingPlatformMB.editMode }"
										disabled="#{workingPlatformMB.selectedQuoteItem.salesCostPart !='Yes'}"
										value="#{workingPlatformMB.selectedQuoteItem.salesCostTypeName}"
										effect="size">
										<f:selectItem itemLabel="NonSC" itemValue="NonSC" />
										<f:selectItem itemLabel="ZINC" itemValue="ZINC" />
										<f:selectItem itemLabel="ZIND" itemValue="ZIND" />

											<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	
										<p:ajax event="change"
											update="d_salescost,
											d_suggestedResale,
											d_quotedPrice,d_quotedMargin"
											listener="#{workingPlatformMB.updateSalesCosteType}" />
									</h:selectOneMenu>
								</div>
							</p:column>

							<p:column styleClass="tdtag">#{dbResource['wq.label.salesCost']}</p:column>
						<p:column  styleClass="tdmandatoryvalue break-all">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.salesCostStr}" />						
							<p:inputText id="d_salescost" rendered="#{workingPlatformMB.editMode}" disabled ="#{!workingPlatformMB.selectedQuoteItem.disableEditableForSalesCost}"  styleClass="inputInRFQDetailDataNumber tdmandatoryvalue" 
							value="#{workingPlatformMB.selectedQuoteItem.salesCostStr}">
							</p:inputText>
						</p:column>	
					
					<p:column styleClass="tdtag">#{dbResource['wq.label.suggested']} #{dbResource['wq.label.resale']}</p:column>
					
					  <p:column styleClass="tdvalue editCell break-all"  colspan="3">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.suggestedResaleStr}" />												
							<p:inputText  rendered="#{workingPlatformMB.editMode}" id="d_suggestedResale"  disabled ="#{!workingPlatformMB.selectedQuoteItem.disableEditableForSalesCost}" styleClass="optional_input_text inputInRFQDetailDataNumber" 
							value="#{workingPlatformMB.selectedQuoteItem.suggestedResaleStr}" >
															
							</p:inputText>
						</p:column>
					</p:row>
					<p:row>
						<p:column styleClass="tdtag">#{dbResource['wq.label.rfqSubmissionDate']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3">#{workingPlatformMB.convertDateTimeToString(workingPlatformMB.selectedQuoteItem.quoteItem.submissionDate)}</p:column>								
						<p:column styleClass="tdtag">#{dbResource['wq.label.ShipToCode']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all">#{workingPlatformMB.selectedQuoteItem.quoteItem.shipToCustomer.customerNumber}</p:column>		
						<p:column styleClass="tdtag">#{dbResource['wq.label.ShipToParty']}</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="3">#{workingPlatformMB.selectedQuoteItem.quoteItem.shipToCustomerFullName}</p:column>
						<p:column styleClass="tdtag">#{dbResource['wq.label.tpMargin']} %</p:column>
						<p:column styleClass="tdvalue readOnlyCell break-all" colspan="1">
							<h:outputText id="d_targetMargin" value="#{workingPlatformMB.selectedQuoteItem.targetMarginStr}">
							</h:outputText>
						</p:column>
						<p:column styleClass="tdtag">#{dbResource['wq.label.quotedMargin']} %</p:column>
						<p:column styleClass="tdvalue tdmandatoryvalue break-all">
							<h:outputText rendered="#{!workingPlatformMB.editMode}" value="#{workingPlatformMB.selectedQuoteItem.resaleMarginStr}" />						
							<p:inputText rendered="#{workingPlatformMB.editMode}"  disabled ="#{workingPlatformMB.selectedQuoteItem.disableEditableForSalesCost}" id="d_quotedMargin" styleClass="inputInRFQDetailDataNumber tdmandatoryvalue" 
							value="#{workingPlatformMB.selectedQuoteItem.resaleMarginStr}" onfocus="removePercentSymbol(this);" onblur="appendPercentSymbol(this);" onkeyup="checkEnterKey(event, this)" onkeypress="return isDouble(event, true)">								
								<f:attribute name="targetQuoteItem" value="#{workingPlatformMB.selectedQuoteItem}" />	                            
								<p:ajax event="change" listener="#{workingPlatformMB.updateQuotedMargin}"
												update="d_cost,
													    d_quotedMargin,
													    d_quotedPrice,
													    d_targetMargin"/>	
