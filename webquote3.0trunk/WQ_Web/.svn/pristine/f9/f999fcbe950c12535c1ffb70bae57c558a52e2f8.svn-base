<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3c.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui">
	
<f:view contentType="text/html">	
    <h:head>
        <f:facet name="first">
            <meta http-equiv="X-UA-Compatible" content="IE=edge" />
            <meta content='text/html; charset=UTF-8' http-equiv="Content-Type"/>
			<meta http-equiv="Cache-Control" Content="no-cache"/>
			<meta http-equiv="Pragma" Content="no-cache"/>
			<meta http-equiv="Expires" Content="0"/>               
            <title>Working Platform Page</title>
        </f:facet>    
    </h:head>
	<h:body>

	<h:form id="form_pendinglist" enctype="multipart/form-data">
			<p:panel header="Working Platform" id="pendinglist">
			<p:growl id="workingPlatformGrowl" sticky="true" showDetail="true"/> 
		    <p:dataTable	widgetVar="datatable_pendinglist_var"
		    				var="rfq"
		    				value="#{workingPlatformMB.workingPlatformItems}" 
		    				rowKey="#{rfq.id}" 
							selection="#{workingPlatformMB.selectedWorkingPlatformItems}"		    				
		                 	filteredValue="#{workingPlatformMB.filteredWorkingPlatformItems}"
		                 	id="datatable_pendinglist"
		    				rowIndexVar="rowIndex"
		    				paginator="true" 
		    				rows="5"
		    				rowsPerPageTemplate="5,10,15"
		                 	paginatorPosition="top"						    				
		                 	editable="true"
		                 	editMode="cell"    	
			                scrollable="true">		    				
               
				<p:ajax event="rowSelect" listener="#{workingPlatformMB.onRowSelect}" update=":form_pendinglist:panelgrid_rfqdetailreference,
																								:panelgrid_freestock,
																								:form_posHistoryList:datatable_posHistoryList,
																								:form_quotationHistoryList:quoteHistorySearch,
																								:form_systemQuotationHistoryList:vendorQuoteHistorySearch"/>
																								
																							
																								 				
				<p:column selectionMode="multiple" styleClass="column_validRfq"/>
               
		        <p:column headerText="Form#" sortBy="#{rfq.quote.formNumber}" filterMatchMode="contains" filterBy="#{rfq.quote.formNumber}" id="formNumber" styleClass="column_formNumber readOnlyCell">
		        	#{rfq.quote.formNumber}
		        </p:column>     
		
		        <p:column headerText="Avnet Quote#" sortBy="#{rfq.alternativeQuoteNumber}" filterMatchMode="contains" filterBy="#{rfq.alternativeQuoteNumber}" id="alternativeQuoteNumber" styleClass="column_rfqCode readOnlyCell">
					<h:outputText value="#{rfq.alternativeQuoteNumber}"/> 
		        </p:column>
	
		        <p:column headerText="Quote&lt;br/&gt;Stage" sortBy="#{rfq.stage}" filterMatchMode="contains" filterBy="#{rfq.stage}" id="stage" styleClass="column_rfqCode readOnlyCell">
					#{rfq.stage}   
		        </p:column>

		        <p:column headerText="Pending&lt;br/&gt;Day" sortBy="#{rfq.pendingDay}" filterMatchMode="contains" filterBy="#{rfq.pendingDay}" id="pendingDay" styleClass="column_icon readOnlyCell">
					#{rfq.pendingDay} 
		        </p:column>
		             	        
		        <p:column headerText="RFQ&lt;br/&gt;Status" sortBy="#{rfq.status}" filterMatchMode="contains" filterBy="#{rfq.status}" id="rfqStatus" styleClass="column_rfqStatus readOnlyCell">
					#{rfq.status}   
		        </p:column>  

		        <p:column headerText="Valid&lt;br/&gt;RFQ" sortBy="#{rfq.validFlagStr}" filterMatchMode="contains" filterBy="#{rfq.validFlagStr}" id="validRfq" styleClass="column_icon readOnlyCell">
					<h:outputText value="#{rfq.validFlag?'Yes':'No'}" />  
		        </p:column> 
		        		                
		        <p:column headerText="MFR&lt;br/&gt;Code" sortBy="#{rfq.requestedMaterial.manufacturer.name}" filterMatchMode="contains" filterBy="#{rfq.requestedMaterial.manufacturer.name}" id="mfr" styleClass="column_mfr_wp readOnlyCell">
					#{rfq.requestedMaterial.manufacturer.name}       
		        </p:column>
		        
		        <p:column headerText="Quote type" sortBy="#{rfq.quoteItem.quoteType}" filterMatchMode="contains" filterBy="#{rfq.quoteItem.quoteType}" styleClass="column_quoteType readOnlyCell">
					#{rfq.quoteItem.quoteType}   
		        </p:column>          
		
		        <p:column headerText="Sold&lt;br/&gt;To Party" sortBy="#{rfq.soldToCustomerFullName}" filterMatchMode="contains" filterBy="#{rfq.soldToCustomerFullName}" id="soldToCustomerName" styleClass="column_soldToCustomerName readOnlyCell">
					#{rfq.soldToCustomerFullName}
		        </p:column>
		
		        <p:column headerText="End&lt;br/&gt;Customer" sortBy="#{rfq.endCustomerFullName}" filterMatchMode="contains" filterBy="#{rfq.endCustomerFullName}" id="endCustomerName" styleClass="column_endCustomerName readOnlyCell">
					#{rfq.endCustomerFullName}
		        </p:column>
		        
		        <p:column headerText="Requested&lt;br/&gt;P/N" sortBy="#{rfq.requestedMaterial.fullMfrPartNumber}" filterMatchMode="contains" filterBy="#{rfq.requestedMaterial.fullMfrPartNumber}" styleClass="column_fullMfrPart readOnlyCell">
					<h:outputText id="requiredPartNumber" value="#{rfq.requestedMaterial.fullMfrPartNumber}" />
		        </p:column>
		
		        <p:column headerText="Required&lt;br/&gt;Qty" sortBy="#{rfq.requestedQty}" filterMatchMode="contains" filterBy="#{rfq.requestedQty}" id="requiredQty" styleClass="column_requiredQty readOnlyCell">
					#{workingPlatformMB.convertNumber(rfq.requestedQty)}
		        </p:column>
		
		        <p:column headerText="EAU" sortBy="#{rfq.eau}" filterMatchMode="contains" filterBy="#{rfq.eau}" id="eau" styleClass="column_requiredQty readOnlyCell">
					#{workingPlatformMB.convertNumber(rfq.eau)}
		        </p:column>
		
		        <p:column headerText="Target&lt;br/&gt;Resale" sortBy="#{rfq.targetResale}" filterMatchMode="contains" filterBy="#{rfq.targetResale}" styleClass="column_targetResale readOnlyCell">
			        <h:outputText id="targetResale" value="#{rfq.targetResale}" rendered="#{rfq.targetResale != 0.0}">
			    		<f:convertNumber minFractionDigits="2" maxFractionDigits="5" />	
			        </h:outputText>
		        </p:column>
		                                                        
		        <p:column headerText="T/P&lt;br/&gt;Margin %" styleClass="column_targetPriceMargin readOnlyCell">
			        <h:outputText id="targetMargin" value="#{rfq.targetMarginStr}">
			        	<f:convertNumber minFractionDigits="2" maxFractionDigits="2"/>			        
			        </h:outputText>
		        </p:column>  
		                             
		        <p:column headerText="Quoted P/N" sortBy="#{rfq.quotedMaterial.fullMfrPartNumber}" filterMatchMode="contains" filterBy="#{rfq.quotedMaterial.fullMfrPartNumber}" id="quotedPartNumber" styleClass="column_quotedPartNumber_wp tdmandatoryvalue">
		            <p:cellEditor>  
		                <f:facet name="output"><h:outputText id="quotedPartNumber_out" value="#{rfq.quotedMaterial.fullMfrPartNumber}" /></f:facet>  
		                <f:facet name="input">
		                	<p:autoComplete id="quotedPartNumber_in" queryDelay="2000" cache="true" cacheTimeout="30000" minQueryLength="5" maxResults="10" label="requested P/N" value="#{rfq.quotedMaterial.fullMfrPartNumber}" completeMethod="#{workingPlatformMB.autoCompletePartList}">
								<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
								<p:ajax event="itemSelect" listener="#{workingPlatformMB.changeQuotedPartNumber}"
												update="mpq_in,mpq_out,
													    moq_in,moq_out,
													    mov_in,mov_out,
													    leadTime_in,leadTime_out,
													    cost_in,cost_out,
													    quotedMargin_in,quotedMargin_out,
													    quotedPrice_in,quotedPrice_out,
													    quotedQty_in,quotedQty_out,
													    validity_in,validity_out,
													    shipmentValidity_in,shipmentValidity_out,
													    targetMargin,
													    costIndicator_in,
													    pmoq,
													    :form_pendinglist:panelgrid_rfqdetailreference">
								</p:ajax>							
							</p:autoComplete>					                    
		                </f:facet>
		            </p:cellEditor>       		        
		        </p:column> 
		        
		        <p:column styleClass="column_icon" headerText="Other P/N&lt;br/&gt;Search">
		        	<p:commandLink id="partNumberSearch_magnifier" actionListener="#{workingPlatformMB.searchForMaterialPopup}" update=":form_pendinglist:alternatePartSearch_panel" onclick="datatable_pendinglist_var.unselectAllRows();datatable_pendinglist_var.selectRow(#{rowIndex});" oncomplete="PF('validation_dialog').show()">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />		        	
						<f:attribute name="targetRowIndex" value="#{rowIndex}" />		        	
		        		<h:graphicImage url="./../resources/images/icon_magnifier.png" styleClass="iconLink"/>				        	
		        	</p:commandLink> 				        
				</p:column>	
		                		        
		        <p:column headerText="Quoted&lt;br/&gt;Margin %" sortBy="#{rfq.resaleMargin}" filterMatchMode="contains" filterBy="#{rfq.resaleMargin}" id="quotedMargin" styleClass="column_quotedMargin editCell">
					<p:cellEditor>
						<f:facet name="output">
							<h:outputText id="quotedMargin_out" value="#{rfq.resaleMargin}">
			        			<f:convertNumber minFractionDigits="2" maxFractionDigits="2"/>			        							
							</h:outputText>
						</f:facet>
						<f:facet name="input">
							<p:inputText id="quotedMargin_in" value="#{rfq.resaleMargin}">
			        			<f:convertNumber minFractionDigits="2" maxFractionDigits="2"/>			        														
								<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
								<p:ajax event="change" listener="#{workingPlatformMB.updateQuotedMargin}"
												update="mpq_in,mpq_out,
													    moq_in,moq_out,
													    mov_in,mov_out,
													    leadTime_in,leadTime_out,
													    cost_in,cost_out,
													    quotedMargin_in,quotedMargin_out,
													    quotedPrice_in,quotedPrice_out,
													    quotedQty_in,quotedQty_out,
													    validity_in,validity_out,
													    shipmentValidity_in,shipmentValidity_out,
													    targetMargin,
													    costIndicator_in,
													    pmoq,
													    :form_pendinglist:panelgrid_rfqdetailreference"/>								
							</p:inputText>																						
						</f:facet>
					</p:cellEditor>           
		        </p:column> 
		                                                
		        <p:column headerText="Quoted&lt;br/&gt;Resale" sortBy="#{rfq.quotedPrice}" filterMatchMode="contains" filterBy="#{rfq.quotedPrice}" id="quotedPrice" styleClass="column_quotedPrice tdmandatoryvalue">
					<p:cellEditor>
						<f:facet name="output">
							<h:outputText id="quotedPrice_out" value="#{rfq.quotedPrice}">
     							<f:convertNumber minFractionDigits="2" maxFractionDigits="5"/>							
							</h:outputText>
						</f:facet>
						<f:facet name="input">
							<p:inputText id="quotedPrice_in" value="#{rfq.quotedPrice}" styleClass="input_text">
     							<f:convertNumber minFractionDigits="2" maxFractionDigits="5"/>																
								<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
								<p:ajax event="change" listener="#{workingPlatformMB.updateQuotedPrice}"
												update="mpq_in,mpq_out,
													    moq_in,moq_out,
													    mov_in,mov_out,
													    leadTime_in,leadTime_out,
													    cost_in,cost_out,
													    quotedMargin_in,quotedMargin_out,
													    quotedPrice_in,quotedPrice_out,
													    quotedQty_in,quotedQty_out,
													    validity_in,validity_out,
													    shipmentValidity_in,shipmentValidity_out,
													    targetMargin,
													    costIndicator_in,
													    pmoq,
													    :form_pendinglist:panelgrid_rfqdetailreference"/>								
							</p:inputText>
						</f:facet>
					</p:cellEditor>           
		        </p:column>        
		       
		        <p:column headerText="Cost" sortBy="#{rfq.cost}" filterMatchMode="contains" filterBy="#{rfq.cost}" id="cost" styleClass="column_cost tdmandatoryvalue">
					<p:cellEditor>
						<f:facet name="output">
							<h:outputText id="cost_out" value="#{rfq.cost}">
     							<f:convertNumber minFractionDigits="2" maxFractionDigits="5"/>								
							</h:outputText>
						</f:facet>
						<f:facet name="input">
							<p:inputText id="cost_in" value="#{rfq.cost}" styleClass="input_text">
     							<f:convertNumber minFractionDigits="2" maxFractionDigits="5"/>								
								<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
								<p:ajax event="change" listener="#{workingPlatformMB.updateCost}"
												update="mpq_in,mpq_out,
													    moq_in,moq_out,
													    mov_in,mov_out,
													    leadTime_in,leadTime_out,
													    cost_in,cost_out,
													    quotedMargin_in,quotedMargin_out,
													    quotedPrice_in,quotedPrice_out,
													    quotedQty_in,quotedQty_out,
													    validity_in,validity_out,
													    shipmentValidity_in,shipmentValidity_out,
													    targetMargin,
													    costIndicator_in,
													    pmoq,
													    :form_pendinglist:panelgrid_rfqdetailreference"/>								
							</p:inputText>
						</f:facet>
					</p:cellEditor>       
		        </p:column>
		       
		        <p:column headerText="Cost&lt;br/&gt;Indicator" sortBy="#{rfq.costIndicator}" filterMatchMode="contains" filterBy="#{rfq.costIndicator}" id="costIndicator" styleClass="column_costIndicator tdmandatoryvalue">
	    			<h:selectOneMenu id="costIndicator_in" value="#{rfq.costIndicator}" effect="size">
						<f:selectItems value="#{workingPlatformMB.convertCostIndicatorToSelectItem()}" />
						<f:attribute name="targetQuoteItem" value="#{rfq}" />															    
						<p:ajax event="change" listener="#{workingPlatformMB.switchCostIndicator}"
												update="mpq_in,mpq_out,
													    moq_in,moq_out,
													    mov_in,mov_out,
													    leadTime_in,leadTime_out,
													    cost_in,cost_out,
													    quotedMargin_in,quotedMargin_out,
													    quotedPrice_in,quotedPrice_out,
													    quotedQty_in,quotedQty_out,
													    validity_in,validity_out,
													    shipmentValidity_in,shipmentValidity_out,
													    targetMargin,
													    costIndicator_in,
													    pmoq,
													    :form_pendinglist:panelgrid_rfqdetailreference">
						</p:ajax>
													    
					</h:selectOneMenu>					
		        </p:column> 
		        
		        <p:column styleClass="column_icon" headerText="Cost Indicator Search">
		        	<p:commandLink actionListener="#{workingPlatformMB.searchForPopupPricer}" update=":form_pendinglist:costIndicator_panel" onclick="datatable_pendinglist_var.unselectAllRows();datatable_pendinglist_var.selectRow(#{rowIndex});" oncomplete="PF('costIndicator_dialog').show()">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />			        	
		        		<h:graphicImage url="./../resources/images/icon_magnifier.png" styleClass="iconLink"/>				        	
		        	</p:commandLink> 				        
				</p:column>			         
		               	        
		        <p:column headerText="Qty&lt;br/&gt;Indicator" sortBy="#{rfq.qtyIndicator}" filterMatchMode="contains" filterBy="#{rfq.qtyIndicator}" id="qtyIndicator" styleClass="column_qtyIndicator tdmandatoryvalue">
	    			<h:selectOneMenu id="qtyIndicator_in" value="#{rfq.qtyIndicator}" effect="size">
                        <f:selectItems value="#{workingPlatformMB.qtyIndicatorOptions}"  
                            var="qtyIndicator"    
                            itemLabel="#{rfq.qtyIndicator}"  
                            itemValue="#{rfq.qtyIndicator}" />  
						<p:ajax event="change" listener="#{workingPlatformMB.updateQtyIndicator(rowIndex)}" update=":form_pendinglist:panelgrid_rfqdetailreference,mpq_in,mpq_out,moq_in,moq_out,mov_in,mov_out,leadTime_in,leadTime_out,cost_in,cost_out,quotedMargin_in,quotedMargin_out,targetMargin,costIndicator_in,quotedQty_in,quotedQty_out,pmoq"/>
					</h:selectOneMenu>					        
		        </p:column>  
		        
		        <p:column headerText="Quoted&lt;br/&gt;Qty" sortBy="#{rfq.quotedQty}" filterMatchMode="contains" filterBy="#{rfq.quotedQty}" id="quotedQty" styleClass="column_quotedQty tdmandatoryvalue">
					<p:cellEditor>
						<f:facet name="output">
							<h:outputText id="quotedQty_out" value="#{workingPlatformMB.convertNumber(rfq.quotedQty)}" />
						</f:facet>
						<f:facet name="input">
							<p:inputText id="quotedQty_in" value="#{rfq.quotedQty}" styleClass="input_text" onclick="checkQtyIndicator(#{rowIndex})" onkeyup="checkInteger(this)">
								<p:ajax event="change" listener="#{workingPlatformMB.updateQuotedQty(rowIndex)}"
												update="mpq_in,mpq_out,
													    moq_in,moq_out,
													    mov_in,mov_out,
													    leadTime_in,leadTime_out,
													    cost_in,cost_out,
													    quotedMargin_in,quotedMargin_out,
													    quotedPrice_in,quotedPrice_out,
													    quotedQty_in,quotedQty_out,
													    validity_in,validity_out,
													    shipmentValidity_in,shipmentValidity_out,
													    targetMargin,
													    costIndicator_in,
													    pmoq,
													    :form_pendinglist:panelgrid_rfqdetailreference"/>								
														
							</p:inputText>
						</f:facet>
					</p:cellEditor>           
		        </p:column>  
		                
		        <p:column headerText="Multi&lt;br/&gt;Usage" sortBy="#{rfq.multiUsageFlagStr}" filterMatchMode="contains" filterBy="#{rfq.multiUsageFlagStr}" id="multiUsage" styleClass="column_multiUsage editCell">
	    			<h:selectOneMenu id="multiUsage_in" value="#{rfq.multiUsageFlagStr}" effect="size">
			            <f:selectItem itemLabel="Yes" itemValue="Yes" />  
			            <f:selectItem itemLabel="No" itemValue="No" />  
						<p:ajax event="change" update=":form_pendinglist:panelgrid_rfqdetailreference"/>				            
					</h:selectOneMenu>					
		        </p:column>    
	        
		        <p:column headerText="Quantity Break" id="quantityBreak"  styleClass="column_costIndicator readOnlyCell">
					<h:outputText value="#{rfq.requestedMaterial.programMaterial}" converter="webquote.commodity.Oqmsp" escape="false" />
				</p:column>					        	
					        	
					        		        
		        <p:column headerText="QC External&lt;br/&gt;Comment" styleClass="column_attachmentId">
		        	<p:commandLink id="qc_external_comment" oncomplete="PF('qc_external_comment_dialog').show()" update=":form_pendinglist:qc_external_comment_panel" onclick="datatable_pendinglist_var.unselectAllRows();datatable_pendinglist_var.selectRow(#{rowIndex});" actionListener="#{workingPlatformMB.updateSelectedQuoteItem}">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />															    		        	
		        		<h:graphicImage url="./../resources/images/icon_info.png" styleClass="iconLink" rendered="#{!workingPlatformMB.isEmpty(rfq.aqcc)}"/>
		        		<h:graphicImage url="./../resources/images/icon_info_empty.png" styleClass="iconLink" rendered="#{workingPlatformMB.isEmpty(rfq.aqcc)}"/>
		        	</p:commandLink> 					        		        	
		        </p:column>            
		
		        <p:column headerText="QC Internal&lt;br/&gt;Comment" styleClass="column_attachmentId">
		        	<p:commandLink id="qc_internal_comment" oncomplete="PF('qc_internal_comment_dialog').show()" update=":form_pendinglist:qc_internal_comment_panel" onclick="datatable_pendinglist_var.unselectAllRows();datatable_pendinglist_var.selectRow(#{rowIndex});" actionListener="#{workingPlatformMB.updateSelectedQuoteItem}">
						<f:attribute name="targetQuoteItem" value="#{rfq}" />															    		        	
		        		<h:graphicImage url="./../resources/images/icon_info.png" styleClass="iconLink" rendered="#{!workingPlatformMB.isEmpty(rfq.internalComment)}"/>
		        		<h:graphicImage url="./../resources/images/icon_info_empty.png" styleClass="iconLink" rendered="#{workingPlatformMB.isEmpty(rfq.internalComment)}"/>
		        	</p:commandLink> 					        		        	
		        </p:column>            
		        
		        <p:column headerText="Lead&lt;br/&gt;Time" sortBy="#{rfq.leadTime}" filterMatchMode="contains" filterBy="#{rfq.leadTime}" id="leadTime" styleClass="column_leadTime editCell">
					<p:cellEditor>
						<f:facet name="output">
							<h:outputText id="leadTime_out" value="#{rfq.leadTime}" />
						</f:facet>
						<f:facet name="input">
							<p:inputText id="leadTime_in" value="#{rfq.leadTime}" styleClass="input_text">
								<p:ajax event="change" update=":form_pendinglist:panelgrid_rfqdetailreference"/>						
							</p:inputText>
						</f:facet>
					</p:cellEditor>       
		        </p:column>
		                                        
		        <p:column headerText="MPQ" sortBy="#{rfq.mpq}" filterMatchMode="contains" filterBy="#{rfq.mpq}" id="mpq" styleClass="column_mpq tdmandatoryvalue">
					<p:cellEditor>
						<f:facet name="output">
							<h:outputText id="mpq_out" value="#{rfq.mpq}" />
						</f:facet>
						<f:facet name="input">
							<p:inputText id="mpq_in" value="#{rfq.mpq}" styleClass="input_text" onkeyup="checkInteger(this)">
								<f:attribute name="targetQuoteItem" value="#{rfq}" />																			    
								<p:ajax event="change" listener="#{workingPlatformMB.updatePmoq}"
												update="mpq_in,mpq_out,
													    moq_in,moq_out,
													    mov_in,mov_out,
													    leadTime_in,leadTime_out,
													    cost_in,cost_out,
													    quotedMargin_in,quotedMargin_out,
													    quotedPrice_in,quotedPrice_out,
													    quotedQty_in,quotedQty_out,
													    validity_in,validity_out,
													    shipmentValidity_in,shipmentValidity_out,
													    targetMargin,
													    costIndicator_in,
													    pmoq,
													    :form_pendinglist:panelgrid_rfqdetailreference">
								</p:ajax>							
							</p:inputText>
						</f:facet>
					</p:cellEditor>       
		        </p:column>
		        
		        <p:column headerText="MOQ" sortBy="#{rfq.moq}" filterMatchMode="contains" filterBy="#{rfq.moq}" id="moq" styleClass="column_moq tdmandatoryvalue">
					<p:cellEditor>
						<f:facet name="output">
							<h:outputText id="moq_out" value="#{rfq.moq}" />
						</f:facet>
						<f:facet name="input">
							<p:inputText id="moq_in" value="#{rfq.moq}" styleClass="input_text" onkeyup="checkInteger(this)">
								<f:attribute name="targetQuoteItem" value="#{rfq}" />																			    
								<p:ajax event="change" listener="#{workingPlatformMB.updatePmoq}"
												update="mpq_in,mpq_out,
													    moq_in,moq_out,
													    mov_in,mov_out,
													    leadTime_in,leadTime_out,
													    cost_in,cost_out,
													    quotedMargin_in,quotedMargin_out,
													    quotedPrice_in,quotedPrice_out,
													    quotedQty_in,quotedQty_out,
													    validity_in,validity_out,
													    shipmentValidity_in,shipmentValidity_out,
													    targetMargin,
													    costIndicator_in,
													    pmoq,
													    :form_pendinglist:panelgrid_rfqdetailreference">								
								</p:ajax>							
