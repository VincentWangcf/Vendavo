        <xs:element name="BPMName">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="35"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="ContactChannel">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="512"/>
            </xs:restriction>
