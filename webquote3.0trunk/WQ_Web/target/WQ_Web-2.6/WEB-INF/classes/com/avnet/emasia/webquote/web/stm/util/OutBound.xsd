        <xs:element name="RFQCode">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="9"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="ReqQty">
          <xs:simpleType>
            <xs:restriction base="xs:string">
