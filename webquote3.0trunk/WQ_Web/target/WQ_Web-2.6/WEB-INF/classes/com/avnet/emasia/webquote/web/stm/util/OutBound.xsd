        <xs:element name="FullMPN">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="35"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="HeadRemark">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="140"/>
