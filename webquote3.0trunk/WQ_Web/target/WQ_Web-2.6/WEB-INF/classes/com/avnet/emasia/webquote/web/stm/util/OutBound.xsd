          </xs:simpleType>
        </xs:element>
        <xs:element name="DeliveryStop">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="8"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="ECBPM">
          <xs:simpleType>
            <xs:restriction base="xs:string">
